(4923, 24, 'فَقَالُوْۤا اَبَشَرًا مِّنَّا وَاحِدًا نَّتَّبِعُهٗۤۙ-اِنَّاۤ اِذًا لَّفِیْ ضَلٰلٍ وَّ سُعُرٍ(۲۴)', 0, 2, 9, 27, 7, 1, 54, 'فقالوا أبشرا منا واحدا نتبعه إنا إذا لفي ضلال وسعر', 0),
(4924, 25, 'ءَاُلْقِیَ الذِّكْرُ عَلَیْهِ مِنْۢ بَیْنِنَا بَلْ هُوَ كَذَّابٌ اَشِرٌ(۲۵)', 0, 2, 9, 27, 7, 1, 54, 'أألقي الذكر عليه من بيننا بل هو كذاب أشر', 0),
(4925, 26, 'سَیَعْلَمُوْنَ غَدًا مَّنِ الْكَذَّابُ الْاَشِرُ(۲۶)', 0, 2, 9, 27, 7, 1, 54, 'سيعلمون غدا من الكذاب الأشر', 0),
(4926, 27, 'اِنَّا مُرْسِلُوا النَّاقَةِ فِتْنَةً لَّهُمْ فَارْتَقِبْهُمْ وَ اصْطَبِرْ٘(۲۷)', 0, 2, 9, 27, 7, 1, 54, 'إنا مرسلو الناقة فتنة لهم فارتقبهم واصطبر', 0),
(4927, 28, 'وَ نَبِّئْهُمْ اَنَّ الْمَآءَ قِسْمَةٌۢ بَیْنَهُمْۚ-كُلُّ شِرْبٍ مُّحْتَضَرٌ(۲۸)', 0, 2, 9, 27, 7, 1, 54, 'ونبئهم أن الماء قسمة بينهم ۖ كل شرب محتضر', 0),
(4928, 29, 'فَنَادَوْا صَاحِبَهُمْ فَتَعَاطٰى فَعَقَرَ(۲۹)', 0, 2, 9, 27, 7, 1, 54, 'فنادوا صاحبهم فتعاطى فعقر', 0),
(4929, 30, 'فَكَیْفَ كَانَ عَذَابِیْ وَ نُذُرِ(۳۰)', 0, 2, 9, 27, 7, 1, 54, 'فكيف كان عذابي ونذر', 0),
(4930, 31, 'اِنَّاۤ اَرْسَلْنَا عَلَیْهِمْ صَیْحَةً وَّاحِدَةً فَكَانُوْا كَهَشِیْمِ الْمُحْتَظِرِ(۳۱)', 0, 2, 9, 27, 7, 1, 54, 'إنا أرسلنا عليهم صيحة واحدة فكانوا كهشيم المحتظر', 0),
(4931, 32, 'وَ لَقَدْ یَسَّرْنَا الْقُرْاٰنَ لِلذِّكْرِ فَهَلْ مِنْ مُّدَّكِرٍ(۳۲)', 0, 2, 9, 27, 7, 1, 54, 'ولقد يسرنا القرآن للذكر فهل من مدكر', 0),
(4932, 33, 'كَذَّبَتْ قَوْمُ لُوْطٍۭ بِالنُّذُرِ(۳۳)', 0, 2, 9, 27, 7, 1, 54, 'كذبت قوم لوط بالنذر', 0),
(4933, 34, 'اِنَّاۤ اَرْسَلْنَا عَلَیْهِمْ حَاصِبًا اِلَّاۤ اٰلَ لُوْطٍؕ-نَجَّیْنٰهُمْ بِسَحَرٍۙ(۳۴)', 0, 2, 9, 27, 7, 1, 54, 'إنا أرسلنا عليهم حاصبا إلا آل لوط ۖ نجيناهم بسحر', 0),
(4934, 35, 'نِّعْمَةً مِّنْ عِنْدِنَاؕ-كَذٰلِكَ نَجْزِیْ مَنْ شَكَرَ(۳۵)', 0, 2, 9, 27, 7, 1, 54, 'نعمة من عندنا ۚ كذلك نجزي من شكر', 0),
(4935, 36, 'وَ لَقَدْ اَنْذَرَهُمْ بَطْشَتَنَا فَتَمَارَوْا بِالنُّذُرِ(۳۶)', 0, 2, 9, 27, 7, 1, 54, 'ولقد أنذرهم بطشتنا فتماروا بالنذر', 0),
(4936, 37, 'وَ لَقَدْ رَاوَدُوْهُ عَنْ ضَیْفِهٖ فَطَمَسْنَاۤ اَعْیُنَهُمْ فَذُوْقُوْا عَذَابِیْ وَ نُذُرِ(۳۷)', 0, 2, 9, 27, 7, 1, 54, 'ولقد راودوه عن ضيفه فطمسنا أعينهم فذوقوا عذابي ونذر', 0),
(4937, 38, 'وَ لَقَدْ صَبَّحَهُمْ بُكْرَةً عَذَابٌ مُّسْتَقِرٌّۚ(۳۸)', 0, 2, 9, 27, 7, 1, 54, 'ولقد صبحهم بكرة عذاب مستقر', 0),
(4938, 39, 'فَذُوْقُوْا عَذَابِیْ وَ نُذُرِ(۳۹)', 0, 2, 9, 27, 7, 1, 54, 'فذوقوا عذابي ونذر', 0),
(4939, 40, 'وَ لَقَدْ یَسَّرْنَا الْقُرْاٰنَ لِلذِّكْرِ فَهَلْ مِنْ مُّدَّكِرٍ۠(۴۰)', 0, 2, 9, 27, 7, 1, 54, 'ولقد يسرنا القرآن للذكر فهل من مدكر', 0),
(4940, 41, 'وَ لَقَدْ جَآءَ اٰلَ فِرْعَوْنَ النُّذُرُۚ(۴۱)', 0, 3, 10, 27, 7, 1, 54, 'ولقد جاء آل فرعون النذر', 0),
(4941, 42, 'كَذَّبُوْا بِاٰیٰتِنَا كُلِّهَا فَاَخَذْنٰهُمْ اَخْذَ عَزِیْزٍ مُّقْتَدِرٍ(۴۲)', 0, 3, 10, 27, 7, 1, 54, 'كذبوا بآياتنا كلها فأخذناهم أخذ عزيز مقتدر', 0),
(4942, 43, 'اَكُفَّارُكُمْ خَیْرٌ مِّنْ اُولٰٓىٕكُمْ اَمْ لَكُمْ بَرَآءَةٌ فِی الزُّبُرِۚ(۴۳)', 0, 3, 10, 27, 7, 1, 54, 'أكفاركم خير من أولئكم أم لكم براءة في الزبر', 0),
(4943, 44, 'اَمْ یَقُوْلُوْنَ نَحْنُ جَمِیْعٌ مُّنْتَصِرٌ(۴۴)', 0, 3, 10, 27, 7, 1, 54, 'أم يقولون نحن جميع منتصر', 0),
(4944, 45, 'سَیُهْزَمُ الْجَمْعُ وَ یُوَلُّوْنَ الدُّبُرَ(۴۵)', 0, 3, 10, 27, 7, 1, 54, 'سيهزم الجمع ويولون الدبر', 0),
(4945, 46, 'بَلِ السَّاعَةُ مَوْعِدُهُمْ وَ السَّاعَةُ اَدْهٰى وَ اَمَرُّ(۴۶)', 0, 3, 10, 27, 7, 1, 54, 'بل الساعة موعدهم والساعة أدهى وأمر', 0),
(4946, 47, 'اِنَّ الْمُجْرِمِیْنَ فِیْ ضَلٰلٍ وَّ سُعُرٍۘ(۴۷)', 0, 3, 10, 27, 7, 1, 54, 'إن المجرمين في ضلال وسعر', 0),
(4947, 48, 'یَوْمَ یُسْحَبُوْنَ فِی النَّارِ عَلٰى وُجُوْهِهِمْؕ-ذُوْقُوْا مَسَّ سَقَرَ(۴۸)', 0, 3, 10, 27, 7, 1, 54, 'يوم يسحبون في النار على وجوههم ذوقوا مس سقر', 0),
(4948, 49, 'اِنَّا كُلَّ شَیْءٍ خَلَقْنٰهُ بِقَدَرٍ(۴۹)', 0, 3, 10, 27, 7, 1, 54, 'إنا كل شيء خلقناه بقدر', 0),
(4949, 50, 'وَ مَاۤ اَمْرُنَاۤ اِلَّا وَاحِدَةٌ كَلَمْحٍۭ بِالْبَصَرِ(۵۰)', 0, 3, 10, 27, 7, 1, 54, 'وما أمرنا إلا واحدة كلمح بالبصر', 0),
(4950, 51, 'وَ لَقَدْ اَهْلَكْنَاۤ اَشْیَاعَكُمْ فَهَلْ مِنْ مُّدَّكِرٍ(۵۱)', 0, 3, 10, 27, 7, 1, 54, 'ولقد أهلكنا أشياعكم فهل من مدكر', 0),
(4951, 52, 'وَ كُلُّ شَیْءٍ فَعَلُوْهُ فِی الزُّبُرِ(۵۲)', 0, 3, 10, 27, 7, 1, 54, 'وكل شيء فعلوه في الزبر', 0),
(4952, 53, 'وَ كُلُّ صَغِیْرٍ وَّ كَبِیْرٍ مُّسْتَطَرٌ(۵۳)', 0, 3, 10, 27, 7, 1, 54, 'وكل صغير وكبير مستطر', 0),
(4953, 54, 'اِنَّ الْمُتَّقِیْنَ فِیْ جَنّٰتٍ وَّ نَهَرٍۙ(۵۴)', 0, 3, 10, 27, 7, 1, 54, 'إن المتقين في جنات ونهر', 0),
(4954, 55, 'فِیْ مَقْعَدِ صِدْقٍ عِنْدَ مَلِیْكٍ مُّقْتَدِرٍ۠(۵۵)', 0, 3, 10, 27, 7, 1, 54, 'في مقعد صدق عند مليك مقتدر', 0),
(4955, 0, 'بِسْمِ اللّٰهِ الرَّحْمٰنِ الرَّحِیْمِ', 0, 1, 11, 27, 7, 1, 55, 'بسم الله الرحمن الرحيم', 0),
(4956, 1, 'اَلرَّحْمٰنُۙ(۱)', 0, 1, 11, 27, 7, 1, 55, 'الرحمن', 0),
(4957, 2, 'عَلَّمَ الْقُرْاٰنَؕ(۲)', 0, 1, 11, 27, 7, 1, 55, 'علم القرآن', 0),
(4958, 3, 'خَلَقَ الْاِنْسَانَۙ(۳)', 0, 1, 11, 27, 7, 1, 55, 'خلق الإنسان', 0),
(4959, 4, 'عَلَّمَهُ الْبَیَانَ(۴)', 0, 1, 11, 27, 7, 1, 55, 'علمه البيان', 0),
(4960, 5, 'اَلشَّمْسُ وَ الْقَمَرُ بِحُسْبَانٍۙ (۵)', 0, 1, 11, 27, 7, 1, 55, 'الشمس والقمر بحسبان', 0),
(4961, 6, 'وَّ النَّجْمُ وَ الشَّجَرُ یَسْجُدٰنِ(۶)', 0, 1, 11, 27, 7, 1, 55, 'والنجم والشجر يسجدان', 0),
(4962, 7, 'وَ السَّمَآءَ رَفَعَهَا وَ وَضَعَ الْمِیْزَانَۙ(۷)', 0, 1, 11, 27, 7, 1, 55, 'والسماء رفعها ووضع الميزان', 0),
(4963, 8, 'اَلَّا تَطْغَوْا فِی الْمِیْزَانِ(۸)', 0, 1, 11, 27, 7, 1, 55, 'ألا تطغوا في الميزان', 0),
(4964, 9, 'وَ اَقِیْمُوا الْوَزْنَ بِالْقِسْطِ وَ لَا تُخْسِرُوا الْمِیْزَانَ(۹)', 0, 1, 11, 27, 7, 1, 55, 'وأقيموا الوزن بالقسط ولا تخسروا الميزان', 0),
(4965, 10, 'وَ الْاَرْضَ وَ ضَعَهَا لِلْاَنَامِۙ(۱۰)', 0, 1, 11, 27, 7, 1, 55, 'والأرض وضعها للأنام', 0),
