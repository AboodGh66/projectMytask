(1620, 13, 'قَالَ اِنِّیْ لَیَحْزُنُنِیْۤ اَنْ تَذْهَبُوْا بِهٖ وَ اَخَافُ اَنْ یَّاْكُلَهُ الذِّئْبُ وَ اَنْتُمْ عَنْهُ غٰفِلُوْنَ(۱۳)', 0, 2, 12, 12, 3, 1, 12, 'قال إني ليحزنني أن تذهبوا به وأخاف أن يأكله الذئب وأنتم عنه غافلون', 0),
(1621, 14, 'قَالُوْا لَىٕنْ اَكَلَهُ الذِّئْبُ وَ نَحْنُ عُصْبَةٌ اِنَّاۤ اِذًا لَّخٰسِرُوْنَ(۱۴)', 0, 2, 12, 12, 3, 1, 12, 'قالوا لئن أكله الذئب ونحن عصبة إنا إذا لخاسرون', 0),
(1622, 15, 'فَلَمَّا ذَهَبُوْا بِهٖ وَ اَجْمَعُوْۤا اَنْ یَّجْعَلُوْهُ فِیْ غَیٰبَتِ الْجُبِّۚ-وَ اَوْحَیْنَاۤ اِلَیْهِ لَتُنَبِّئَنَّهُمْ بِاَمْرِهِمْ هٰذَا وَ هُمْ لَا یَشْعُرُوْنَ(۱۵)', 0, 2, 12, 12, 3, 1, 12, 'فلما ذهبوا به وأجمعوا أن يجعلوه في غيابت الجب ۚ وأوحينا إليه لتنبئنهم بأمرهم هذا وهم لا يشعرون', 0),
(1623, 16, 'وَ جَآءُوْۤ اَبَاهُمْ عِشَآءً یَّبْكُوْنَؕ(۱۶)', 0, 2, 12, 12, 3, 1, 12, 'وجاءوا أباهم عشاء يبكون', 0),
(1624, 17, 'قَالُوْا یٰۤاَبَانَاۤ اِنَّا ذَهَبْنَا نَسْتَبِقُ وَ تَرَكْنَا یُوْسُفَ عِنْدَ مَتَاعِنَا فَاَكَلَهُ الذِّئْبُۚ-وَ مَاۤ اَنْتَ بِمُؤْمِنٍ لَّنَا وَ لَوْ كُنَّا صٰدِقِیْنَ(۱۷)', 0, 2, 12, 12, 3, 1, 12, 'قالوا يا أبانا إنا ذهبنا نستبق وتركنا يوسف عند متاعنا فأكله الذئب ۖ وما أنت بمؤمن لنا ولو كنا صادقين', 0),
(1625, 18, 'وَ جَآءُوْ عَلٰى قَمِیْصِهٖ بِدَمٍ كَذِبٍؕ-قَالَ بَلْ سَوَّلَتْ لَكُمْ اَنْفُسُكُمْ اَمْرًاؕ-فَصَبْرٌ جَمِیْلٌؕ-وَ اللّٰهُ الْمُسْتَعَانُ عَلٰى مَا تَصِفُوْنَ(۱۸)', 0, 2, 12, 12, 3, 1, 12, 'وجاءوا على قميصه بدم كذب ۚ قال بل سولت لكم أنفسكم أمرا ۖ فصبر جميل ۖ والله المستعان على ما تصفون', 0),
(1626, 19, 'وَ جَآءَتْ سَیَّارَةٌ فَاَرْسَلُوْا وَارِدَهُمْ فَاَدْلٰى دَلْوَهٗؕ-قَالَ یٰبُشْرٰى هٰذَا غُلٰمٌؕ-وَ اَسَرُّوْهُ بِضَاعَةًؕ-وَ اللّٰهُ عَلِیْمٌۢ بِمَا یَعْمَلُوْنَ(۱۹)', 0, 2, 12, 12, 3, 1, 12, 'وجاءت سيارة فأرسلوا واردهم فأدلى دلوه ۖ قال يا بشرى هذا غلام ۚ وأسروه بضاعة ۚ والله عليم بما يعملون', 0),
(1627, 20, 'وَ شَرَوْهُ بِثَمَنٍۭ بَخْسٍ دَرَاهِمَ مَعْدُوْدَةٍۚ-وَ كَانُوْا فِیْهِ مِنَ الزَّاهِدِیْنَ۠(۲۰)', 0, 2, 12, 12, 3, 1, 12, 'وشروه بثمن بخس دراهم معدودة وكانوا فيه من الزاهدين', 0),
(1628, 21, 'وَ قَالَ الَّذِی اشْتَرٰىهُ مِنْ مِّصْرَ لِامْرَاَتِهٖۤ اَكْرِمِیْ مَثْوٰىهُ عَسٰۤى اَنْ یَّنْفَعَنَاۤ اَوْ نَتَّخِذَهٗ وَلَدًاؕ-وَ كَذٰلِكَ مَكَّنَّا لِیُوْسُفَ فِی الْاَرْضِ٘-وَ لِنُعَلِّمَهٗ مِنْ تَاْوِیْلِ الْاَحَادِیْثِؕ-وَ اللّٰهُ غَالِبٌ عَلٰۤى اَمْرِهٖ وَ لٰكِنَّ اَكْثَرَ النَّاسِ لَا یَعْلَمُوْنَ(۲۱)', 0, 3, 13, 12, 3, 1, 12, 'وقال الذي اشتراه من مصر لامرأته أكرمي مثواه عسى أن ينفعنا أو نتخذه ولدا ۚ وكذلك مكنا ليوسف في الأرض ولنعلمه من تأويل الأحاديث ۚ والله غالب على أمره ولكن أكثر الناس لا يعلمون', 0),
(1629, 22, 'وَ لَمَّا بَلَغَ اَشُدَّهٗۤ اٰتَیْنٰهُ حُكْمًا وَّ عِلْمًاؕ-وَ كَذٰلِكَ نَجْزِی الْمُحْسِنِیْنَ(۲۲)', 0, 3, 13, 12, 3, 1, 12, 'ولما بلغ أشده آتيناه حكما وعلما ۚ وكذلك نجزي المحسنين', 0),
(1630, 23, 'وَ رَاوَدَتْهُ الَّتِیْ هُوَ فِیْ بَیْتِهَا عَنْ نَّفْسِهٖ وَ غَلَّقَتِ الْاَبْوَابَ وَ قَالَتْ هَیْتَ لَكَؕ-قَالَ مَعَاذَ اللّٰهِ اِنَّهٗ رَبِّیْۤ اَحْسَنَ مَثْوَایَؕ-اِنَّهٗ لَا یُفْلِحُ الظّٰلِمُوْنَ(۲۳)', 0, 3, 13, 12, 3, 1, 12, 'وراودته التي هو في بيتها عن نفسه وغلقت الأبواب وقالت هيت لك ۚ قال معاذ الله ۖ إنه ربي أحسن مثواي ۖ إنه لا يفلح الظالمون', 0),
(1631, 24, 'وَ لَقَدْ هَمَّتْ بِهٖۚ-وَ هَمَّ بِهَا لَوْ لَاۤ اَنْ رَّاٰ بُرْهَانَ رَبِّهٖؕ-كَذٰلِكَ لِنَصْرِفَ عَنْهُ السُّوْٓءَ وَ الْفَحْشَآءَؕ-اِنَّهٗ مِنْ عِبَادِنَا الْمُخْلَصِیْنَ(۲۴)', 0, 3, 13, 12, 3, 1, 12, 'ولقد همت به ۖ وهم بها لولا أن رأى برهان ربه ۚ كذلك لنصرف عنه السوء والفحشاء ۚ إنه من عبادنا المخلصين', 0),
(1632, 25, 'وَ اسْتَبَقَا الْبَابَ وَ قَدَّتْ قَمِیْصَهٗ مِنْ دُبُرٍ وَّ اَلْفَیَا سَیِّدَهَا لَدَا الْبَابِؕ-قَالَتْ مَا جَزَآءُ مَنْ اَرَادَ بِاَهْلِكَ سُوْٓءًا اِلَّاۤ اَنْ یُّسْجَنَ اَوْ عَذَابٌ اَلِیْمٌ(۲۵)', 0, 3, 13, 12, 3, 1, 12, 'واستبقا الباب وقدت قميصه من دبر وألفيا سيدها لدى الباب ۚ قالت ما جزاء من أراد بأهلك سوءا إلا أن يسجن أو عذاب أليم', 0),
(1633, 26, 'قَالَ هِیَ رَاوَدَتْنِیْ عَنْ نَّفْسِیْ وَ شَهِدَ شَاهِدٌ مِّنْ اَهْلِهَاۚ-اِنْ كَانَ قَمِیْصُهٗ قُدَّ مِنْ قُبُلٍ فَصَدَقَتْ وَ هُوَ مِنَ الْكٰذِبِیْنَ(۲۶)', 0, 3, 13, 12, 3, 1, 12, 'قال هي راودتني عن نفسي ۚ وشهد شاهد من أهلها إن كان قميصه قد من قبل فصدقت وهو من الكاذبين', 0),
(1634, 27, 'وَ اِنْ كَانَ قَمِیْصُهٗ قُدَّ مِنْ دُبُرٍ فَكَذَبَتْ وَ هُوَ مِنَ الصّٰدِقِیْنَ(۲۷)', 0, 3, 13, 12, 3, 1, 12, 'وإن كان قميصه قد من دبر فكذبت وهو من الصادقين', 0),
(1635, 28, 'فَلَمَّا رَاٰ قَمِیْصَهٗ قُدَّ مِنْ دُبُرٍ قَالَ اِنَّهٗ مِنْ كَیْدِكُنَّؕ-اِنَّ كَیْدَكُنَّ عَظِیْمٌ(۲۸)', 0, 3, 13, 12, 3, 1, 12, 'فلما رأى قميصه قد من دبر قال إنه من كيدكن ۖ إن كيدكن عظيم', 0),
(1636, 29, 'یُوْسُفُ اَعْرِضْ عَنْ هٰذَاٚ-وَ اسْتَغْفِرِیْ لِذَنْۢبِكِ ۚۖ-اِنَّكِ كُنْتِ مِنَ الْخٰطِـٕیْنَ۠(۲۹)', 0, 3, 13, 12, 3, 1, 12, 'يوسف أعرض عن هذا ۚ واستغفري لذنبك ۖ إنك كنت من الخاطئين', 0),
(1637, 30, 'وَ قَالَ نِسْوَةٌ فِی الْمَدِیْنَةِ امْرَاَتُ الْعَزِیْزِ تُرَاوِدُ فَتٰىهَا عَنْ نَّفْسِهٖۚ-قَدْ شَغَفَهَا حُبًّاؕ-اِنَّا لَنَرٰىهَا فِیْ ضَلٰلٍ مُّبِیْنٍ(۳۰)', 0, 4, 14, 12, 3, 1, 12, 'وقال نسوة في المدينة امرأت العزيز تراود فتاها عن نفسه ۖ قد شغفها حبا ۖ إنا لنراها في ضلال مبين', 0),
(1638, 31, 'فَلَمَّا سَمِعَتْ بِمَكْرِهِنَّ اَرْسَلَتْ اِلَیْهِنَّ وَ اَعْتَدَتْ لَهُنَّ مُتَّكَاً وَّ اٰتَتْ كُلَّ وَاحِدَةٍ مِّنْهُنَّ سِكِّیْنًا وَّ قَالَتِ اخْرُ جْ عَلَیْهِنَّۚ-فَلَمَّا رَاَیْنَهٗۤ اَكْبَرْنَهٗ وَ قَطَّعْنَ اَیْدِیَهُنَّ وَ قُلْنَ حَاشَ لِلّٰهِ مَا هٰذَا بَشَرًاؕ-اِنْ هٰذَاۤ اِلَّا مَلَكٌ كَرِیْمٌ(۳۱)', 0, 4, 14, 12, 3, 1, 12, 'فلما سمعت بمكرهن أرسلت إليهن وأعتدت لهن متكأ وآتت كل واحدة منهن سكينا وقالت اخرج عليهن ۖ فلما رأينه أكبرنه وقطعن أيديهن وقلن حاش لله ما هذا بشرا إن هذا إلا ملك كريم', 0),
(1639, 32, 'قَالَتْ فَذٰلِكُنَّ الَّذِیْ لُمْتُنَّنِیْ فِیْهِؕ-وَ لَقَدْ رَاوَدْتُّهٗ عَنْ نَّفْسِهٖ فَاسْتَعْصَمَؕ-وَ لَىٕنْ لَّمْ یَفْعَلْ مَاۤ اٰمُرُهٗ لَیُسْجَنَنَّ وَ لَیَكُوْنًا مِّنَ الصّٰغِرِیْنَ(۳۲)', 0, 4, 14, 12, 3, 1, 12, 'قالت فذلكن الذي لمتنني فيه ۖ ولقد راودته عن نفسه فاستعصم ۖ ولئن لم يفعل ما آمره ليسجنن وليكونا من الصاغرين', 0),
(1640, 33, 'قَالَ رَبِّ السِّجْنُ اَحَبُّ اِلَیَّ مِمَّا یَدْعُوْنَنِیْۤ اِلَیْهِۚ-وَ اِلَّا تَصْرِفْ عَنِّیْ كَیْدَهُنَّ اَصْبُ اِلَیْهِنَّ وَ اَكُنْ مِّنَ الْجٰهِلِیْنَ(۳۳)', 0, 4, 14, 12, 3, 1, 12, 'قال رب السجن أحب إلي مما يدعونني إليه ۖ وإلا تصرف عني كيدهن أصب إليهن وأكن من الجاهلين', 0),
(1641, 34, 'فَاسْتَجَابَ لَهٗ رَبُّهٗ فَصَرَفَ عَنْهُ كَیْدَهُنَّؕ-اِنَّهٗ هُوَ السَّمِیْعُ الْعَلِیْمُ(۳۴)', 0, 4, 14, 12, 3, 1, 12, 'فاستجاب له ربه فصرف عنه كيدهن ۚ إنه هو السميع العليم', 0),
(1642, 35, 'ثُمَّ بَدَا لَهُمْ مِّنْۢ بَعْدِ مَا رَاَوُا الْاٰیٰتِ لَیَسْجُنُنَّهٗ حَتّٰى حِیْنٍ۠(۳۵)', 0, 4, 14, 12, 3, 1, 12, 'ثم بدا لهم من بعد ما رأوا الآيات ليسجننه حتى حين', 0),
(1643, 36, 'وَ دَخَلَ مَعَهُ السِّجْنَ فَتَیٰنِؕ-قَالَ اَحَدُهُمَاۤ اِنِّیْۤ اَرٰىنِیْۤ اَعْصِرُ خَمْرًاۚ-وَ قَالَ الْاٰخَرُ اِنِّیْۤ اَرٰىنِیْۤ اَحْمِلُ فَوْقَ رَاْسِیْ خُبْزًا تَاْكُلُ الطَّیْرُ مِنْهُؕ-نَبِّئْنَا بِتَاْوِیْلِهٖۚ-اِنَّا نَرٰىكَ مِنَ الْمُحْسِنِیْنَ(۳۶) ', 0, 5, 15, 12, 3, 1, 12, 'ودخل معه السجن فتيان ۖ قال أحدهما إني أراني أعصر خمرا ۖ وقال الآخر إني أراني أحمل فوق رأسي خبزا تأكل الطير منه ۖ نبئنا بتأويله ۖ إنا نراك من المحسنين', 0),
(1644, 37, 'قَالَ لَا یَاْتِیْكُمَا طَعَامٌ تُرْزَقٰنِهٖۤ اِلَّا نَبَّاْتُكُمَا بِتَاْوِیْلِهٖ قَبْلَ اَنْ یَّاْتِیَكُمَاؕ-ذٰلِكُمَا مِمَّا عَلَّمَنِیْ رَبِّیْؕ-اِنِّیْ تَرَكْتُ مِلَّةَ قَوْمٍ لَّا یُؤْمِنُوْنَ بِاللّٰهِ وَ هُمْ بِالْاٰخِرَةِ هُمْ كٰفِرُوْنَ(۳۷)', 0, 5, 15, 12, 3, 1, 12, 'قال لا يأتيكما طعام ترزقانه إلا نبأتكما بتأويله قبل أن يأتيكما ۚ ذلكما مما علمني ربي ۚ إني تركت ملة قوم لا يؤمنون بالله وهم بالآخرة هم كافرون', 0),
(1645, 38, 'وَ اتَّبَعْتُ مِلَّةَ اٰبَآءِیْۤ اِبْرٰهِیْمَ وَ اِسْحٰقَ وَ یَعْقُوْبَؕ-مَا كَانَ لَنَاۤ اَنْ نُّشْرِكَ بِاللّٰهِ مِنْ شَیْءٍؕ-ذٰلِكَ مِنْ فَضْلِ اللّٰهِ عَلَیْنَا وَ عَلَى النَّاسِ وَ لٰكِنَّ اَكْثَرَ النَّاسِ لَا یَشْكُرُوْنَ(۳۸)', 0, 5, 15, 12, 3, 1, 12, 'واتبعت ملة آبائي إبراهيم وإسحاق ويعقوب ۚ ما كان لنا أن نشرك بالله من شيء ۚ ذلك من فضل الله علينا وعلى الناس ولكن أكثر الناس لا يشكرون', 0),
(1646, 39, 'یٰصَاحِبَیِ السِّجْنِ ءَاَرْبَابٌ مُّتَفَرِّقُوْنَ خَیْرٌ اَمِ اللّٰهُ الْوَاحِدُ الْقَهَّارُؕ(۳۹)', 0, 5, 15, 12, 3, 1, 12, 'يا صاحبي السجن أأرباب متفرقون خير أم الله الواحد القهار', 0),
(1647, 40, 'مَا تَعْبُدُوْنَ مِنْ دُوْنِهٖۤ اِلَّاۤ اَسْمَآءً سَمَّیْتُمُوْهَاۤ اَنْتُمْ وَ اٰبَآؤُكُمْ مَّاۤ اَنْزَلَ اللّٰهُ بِهَا مِنْ سُلْطٰنٍؕ-اِنِ الْحُكْمُ اِلَّا لِلّٰهِؕ-اَمَرَ اَلَّا تَعْبُدُوْۤا اِلَّاۤ اِیَّاهُؕ-ذٰلِكَ الدِّیْنُ الْقَیِّمُ وَ لٰكِنَّ اَكْثَرَ النَّاسِ لَا یَعْلَمُوْنَ(۴۰)', 0, 5, 15, 12, 3, 1, 12, 'ما تعبدون من دونه إلا أسماء سميتموها أنتم وآباؤكم ما أنزل الله بها من سلطان ۚ إن الحكم إلا لله ۚ أمر ألا تعبدوا إلا إياه ۚ ذلك الدين القيم ولكن أكثر الناس لا يعلمون', 0),
(1648, 41, 'یٰصَاحِبَیِ السِّجْنِ اَمَّاۤ اَحَدُكُمَا فَیَسْقِیْ رَبَّهٗ خَمْرًاۚ-وَ اَمَّا الْاٰخَرُ فَیُصْلَبُ فَتَاْكُلُ الطَّیْرُ مِنْ رَّاْسِهٖؕ-قُضِیَ الْاَمْرُ الَّذِیْ فِیْهِ تَسْتَفْتِیٰنِؕ(۴۱)', 0, 5, 15, 12, 3, 1, 12, 'يا صاحبي السجن أما أحدكما فيسقي ربه خمرا ۖ وأما الآخر فيصلب فتأكل الطير من رأسه ۚ قضي الأمر الذي فيه تستفتيان', 0),
(1649, 42, 'وَ قَالَ لِلَّذِیْ ظَنَّ اَنَّهٗ نَاجٍ مِّنْهُمَا اذْكُرْنِیْ عِنْدَ رَبِّكَ٘-فَاَنْسٰىهُ الشَّیْطٰنُ ذِكْرَ رَبِّهٖ فَلَبِثَ فِی السِّجْنِ بِضْعَ سِنِیْنَ۠(۴۲)', 0, 5, 15, 12, 3, 1, 12, 'وقال للذي ظن أنه ناج منهما اذكرني عند ربك فأنساه الشيطان ذكر ربه فلبث في السجن بضع سنين', 0),
(1650, 43, 'وَ قَالَ الْمَلِكُ اِنِّیْۤ اَرٰى سَبْعَ بَقَرٰتٍ سِمَانٍ یَّاْكُلُهُنَّ سَبْعٌ عِجَافٌ وَّ سَبْعَ سُنْۢبُلٰتٍ خُضْرٍ وَّ اُخَرَ یٰبِسٰتٍؕ-یٰۤاَیُّهَا الْمَلَاُ اَفْتُوْنِیْ فِیْ رُءْیَایَ اِنْ كُنْتُمْ لِلرُّءْیَا تَعْبُرُوْنَ(۴۳)', 0, 6, 16, 12, 3, 1, 12, 'وقال الملك إني أرى سبع بقرات سمان يأكلهن سبع عجاف وسبع سنبلات خضر وأخر يابسات ۖ يا أيها الملأ أفتوني في رؤياي إن كنتم للرؤيا تعبرون', 0),
(1651, 44, 'قَالُوْۤا اَضْغَاثُ اَحْلَامٍۚ-وَ مَا نَحْنُ بِتَاْوِیْلِ الْاَحْلَامِ بِعٰلِمِیْنَ(۴۴)', 0, 6, 16, 12, 3, 1, 12, 'قالوا أضغاث أحلام ۖ وما نحن بتأويل الأحلام بعالمين', 0),
(1652, 45, 'وَ قَالَ الَّذِیْ نَجَا مِنْهُمَا وَ ادَّكَرَ بَعْدَ اُمَّةٍ اَنَا اُنَبِّئُكُمْ بِتَاْوِیْلِهٖ فَاَرْسِلُوْنِ(۴۵)', 0, 6, 16, 12, 3, 1, 12, 'وقال الذي نجا منهما وادكر بعد أمة أنا أنبئكم بتأويله فأرسلون', 0),
(1653, 46, 'یُوْسُفُ اَیُّهَا الصِّدِّیْقُ اَفْتِنَا فِیْ سَبْعِ بَقَرٰتٍ سِمَانٍ یَّاْكُلُهُنَّ سَبْعٌ عِجَافٌ وَّ سَبْعِ سُنْۢبُلٰتٍ خُضْرٍ وَّ اُخَرَ یٰبِسٰتٍۙ-لَّعَلِّیْۤ اَرْجِعُ اِلَى النَّاسِ لَعَلَّهُمْ یَعْلَمُوْنَ(۴۶)', 0, 6, 16, 12, 3, 1, 12, 'يوسف أيها الصديق أفتنا في سبع بقرات سمان يأكلهن سبع عجاف وسبع سنبلات خضر وأخر يابسات لعلي أرجع إلى الناس لعلهم يعلمون', 0),
(1654, 47, 'قَالَ تَزْرَعُوْنَ سَبْعَ سِنِیْنَ دَاَبًاۚ-فَمَا حَصَدْتُّمْ فَذَرُوْهُ فِیْ سُنْۢبُلِهٖۤ اِلَّا قَلِیْلًا مِّمَّا تَاْكُلُوْنَ(۴۷)', 0, 6, 16, 12, 3, 1, 12, 'قال تزرعون سبع سنين دأبا فما حصدتم فذروه في سنبله إلا قليلا مما تأكلون', 0),
(1655, 48, 'ثُمَّ یَاْتِیْ مِنْۢ بَعْدِ ذٰلِكَ سَبْعٌ شِدَادٌ یَّاْكُلْنَ مَا قَدَّمْتُمْ لَهُنَّ اِلَّا قَلِیْلًا مِّمَّا تُحْصِنُوْنَ(۴۸)', 0, 6, 16, 12, 3, 1, 12, 'ثم يأتي من بعد ذلك سبع شداد يأكلن ما قدمتم لهن إلا قليلا مما تحصنون', 0),
(1656, 49, 'ثُمَّ یَاْتِیْ مِنْۢ بَعْدِ ذٰلِكَ عَامٌ فِیْهِ یُغَاثُ النَّاسُ وَ فِیْهِ یَعْصِرُوْنَ۠(۴۹)', 0, 6, 16, 12, 3, 1, 12, 'ثم يأتي من بعد ذلك عام فيه يغاث الناس وفيه يعصرون', 0),
(1657, 50, 'وَ قَالَ الْمَلِكُ ائْتُوْنِیْ بِهٖۚ-فَلَمَّا جَآءَهُ الرَّسُوْلُ قَالَ ارْجِعْ اِلٰى رَبِّكَ فَسْــٴَـلْهُ مَا بَالُ النِّسْوَةِ الّٰتِیْ قَطَّعْنَ اَیْدِیَهُنَّؕ-اِنَّ رَبِّیْ بِكَیْدِهِنَّ عَلِیْمٌ(۵۰)', 0, 7, 17, 12, 3, 1, 12, 'وقال الملك ائتوني به ۖ فلما جاءه الرسول قال ارجع إلى ربك فاسأله ما بال النسوة اللاتي قطعن أيديهن ۚ إن ربي بكيدهن عليم', 0),
(1658, 51, 'قَالَ مَا خَطْبُكُنَّ اِذْ رَاوَدْتُّنَّ یُوْسُفَ عَنْ نَّفْسِهٖؕ-قُلْنَ حَاشَ لِلّٰهِ مَا عَلِمْنَا عَلَیْهِ مِنْ سُوْٓءٍؕ-قَالَتِ امْرَاَتُ الْعَزِیْزِ الْــٴٰـنَ حَصْحَصَ الْحَقُّ٘-اَنَا رَاوَدْتُّهٗ عَنْ نَّفْسِهٖ وَ اِنَّهٗ لَمِنَ الصّٰدِقِیْنَ(۵۱)', 0, 7, 17, 12, 3, 1, 12, 'قال ما خطبكن إذ راودتن يوسف عن نفسه ۚ قلن حاش لله ما علمنا عليه من سوء ۚ قالت امرأت العزيز الآن حصحص الحق أنا راودته عن نفسه وإنه لمن الصادقين', 0),
(1659, 52, 'ذٰلِكَ لِیَعْلَمَ اَنِّیْ لَمْ اَخُنْهُ بِالْغَیْبِ وَ اَنَّ اللّٰهَ لَا یَهْدِیْ كَیْدَ الْخَآىٕنِیْنَ(۵۲)', 0, 7, 17, 12, 3, 1, 12, 'ذلك ليعلم أني لم أخنه بالغيب وأن الله لا يهدي كيد الخائنين', 0),
(1660, 53, 'وَ مَاۤ اُبَرِّئُ نَفْسِیْۚ-اِنَّ النَّفْسَ لَاَمَّارَةٌۢ بِالسُّوْٓءِ اِلَّا مَا رَحِمَ رَبِّیْؕ-اِنَّ رَبِّیْ غَفُوْرٌ رَّحِیْمٌ(۵۳)', 0, 7, 1, 13, 3, 1, 12, 'وما أبرئ نفسي ۚ إن النفس لأمارة بالسوء إلا ما رحم ربي ۚ إن ربي غفور رحيم', 0),
(1661, 54, 'وَ قَالَ الْمَلِكُ ائْتُوْنِیْ بِهٖۤ اَسْتَخْلِصْهُ لِنَفْسِیْۚ-فَلَمَّا كَلَّمَهٗ قَالَ اِنَّكَ الْیَوْمَ لَدَیْنَا مَكِیْنٌ اَمِیْنٌ(۵۴)', 0, 7, 1, 13, 3, 1, 12, 'وقال الملك ائتوني به أستخلصه لنفسي ۖ فلما كلمه قال إنك اليوم لدينا مكين أمين', 0),
(1662, 55, 'قَالَ اجْعَلْنِیْ عَلٰى خَزَآىٕنِ الْاَرْضِۚ-اِنِّیْ حَفِیْظٌ عَلِیْمٌ(۵۵)', 0, 7, 1, 13, 3, 1, 12, 'قال اجعلني على خزائن الأرض ۖ إني حفيظ عليم', 0),
(1663, 56, 'وَ كَذٰلِكَ مَكَّنَّا لِیُوْسُفَ فِی الْاَرْضِۚ-یَتَبَوَّاُ مِنْهَا حَیْثُ یَشَآءُؕ-نُصِیْبُ بِرَحْمَتِنَا مَنْ نَّشَآءُ وَ لَا نُضِیْعُ اَجْرَ الْمُحْسِنِیْنَ(۵۶)', 0, 7, 1, 13, 3, 1, 12, 'وكذلك مكنا ليوسف في الأرض يتبوأ منها حيث يشاء ۚ نصيب برحمتنا من نشاء ۖ ولا نضيع أجر المحسنين', 0),
(1664, 57, 'وَ لَاَجْرُ الْاٰخِرَةِ خَیْرٌ لِّلَّذِیْنَ اٰمَنُوْا وَ كَانُوْا یَتَّقُوْنَ۠(۵۷)', 0, 7, 1, 13, 3, 1, 12, 'ولأجر الآخرة خير للذين آمنوا وكانوا يتقون', 0),
(1665, 58, 'وَ جَآءَ اِخْوَةُ یُوْسُفَ فَدَخَلُوْا عَلَیْهِ فَعَرَفَهُمْ وَ هُمْ لَهٗ مُنْكِرُوْنَ(۵۸)', 0, 8, 2, 13, 3, 1, 12, 'وجاء إخوة يوسف فدخلوا عليه فعرفهم وهم له منكرون', 0),
(1666, 59, 'وَ لَمَّا جَهَّزَهُمْ بِجَهَازِهِمْ قَالَ ائْتُوْنِیْ بِاَخٍ لَّكُمْ مِّنْ اَبِیْكُمْۚ-اَلَا تَرَوْنَ اَنِّیْۤ اُوْفِی الْكَیْلَ وَ اَنَا خَیْرُ الْمُنْزِلِیْنَ(۵۹)', 0, 8, 2, 13, 3, 1, 12, 'ولما جهزهم بجهازهم قال ائتوني بأخ لكم من أبيكم ۚ ألا ترون أني أوفي الكيل وأنا خير المنزلين', 0),
(1667, 60, 'فَاِنْ لَّمْ تَاْتُوْنِیْ بِهٖ فَلَا كَیْلَ لَكُمْ عِنْدِیْ وَ لَا تَقْرَبُوْنِ(۶۰)', 0, 8, 2, 13, 3, 1, 12, 'فإن لم تأتوني به فلا كيل لكم عندي ولا تقربون', 0),
(1668, 61, 'قَالُوْا سَنُرَاوِدُ عَنْهُ اَبَاهُ وَ اِنَّا لَفٰعِلُوْنَ(۶۱)', 0, 8, 2, 13, 3, 1, 12, 'قالوا سنراود عنه أباه وإنا لفاعلون', 0),
(1669, 62, 'وَ قَالَ لِفِتْیٰنِهِ اجْعَلُوْا بِضَاعَتَهُمْ فِیْ رِحَالِهِمْ لَعَلَّهُمْ یَعْرِفُوْنَهَاۤ اِذَا انْقَلَبُوْۤا اِلٰۤى اَهْلِهِمْ لَعَلَّهُمْ یَرْجِعُوْنَ(۶۲)', 0, 8, 2, 13, 3, 1, 12, 'وقال لفتيانه اجعلوا بضاعتهم في رحالهم لعلهم يعرفونها إذا انقلبوا إلى أهلهم لعلهم يرجعون', 0),
(1670, 63, 'فَلَمَّا رَجَعُوْۤا اِلٰۤى اَبِیْهِمْ قَالُوْا یٰۤاَبَانَا مُنِعَ مِنَّا الْكَیْلُ فَاَرْسِلْ مَعَنَاۤ اَخَانَا نَكْتَلْ وَ اِنَّا لَهٗ لَحٰفِظُوْنَ(۶۳)', 0, 8, 2, 13, 3, 1, 12, 'فلما رجعوا إلى أبيهم قالوا يا أبانا منع منا الكيل فأرسل معنا أخانا نكتل وإنا له لحافظون', 0),
(1671, 64, 'قَالَ هَلْ اٰمَنُكُمْ عَلَیْهِ اِلَّا كَمَاۤ اَمِنْتُكُمْ عَلٰۤى اَخِیْهِ مِنْ قَبْلُؕ-فَاللّٰهُ خَیْرٌ حٰفِظًا۪-وَّ هُوَ اَرْحَمُ الرّٰحِمِیْنَ(۶۴)', 0, 8, 2, 13, 3, 1, 12, 'قال هل آمنكم عليه إلا كما أمنتكم على أخيه من قبل ۖ فالله خير حافظا ۖ وهو أرحم الراحمين', 0),
(1672, 65, 'وَ لَمَّا فَتَحُوْا مَتَاعَهُمْ وَجَدُوْا بِضَاعَتَهُمْ رُدَّتْ اِلَیْهِمْؕ-قَالُوْا یٰۤاَبَانَا مَا نَبْغِیْؕ-هٰذِهٖ بِضَاعَتُنَا رُدَّتْ اِلَیْنَاۚ-وَ نَمِیْرُ اَهْلَنَا وَ نَحْفَظُ اَخَانَا وَ نَزْدَادُ كَیْلَ بَعِیْرٍؕ-ذٰلِكَ كَیْلٌ یَّسِیْرٌ(۶۵)', 0, 8, 2, 13, 3, 1, 12, 'ولما فتحوا متاعهم وجدوا بضاعتهم ردت إليهم ۖ قالوا يا أبانا ما نبغي ۖ هذه بضاعتنا ردت إلينا ۖ ونمير أهلنا ونحفظ أخانا ونزداد كيل بعير ۖ ذلك كيل يسير', 0),
(1673, 66, 'قَالَ لَنْ اُرْسِلَهٗ مَعَكُمْ حَتّٰى تُؤْتُوْنِ مَوْثِقًا مِّنَ اللّٰهِ لَتَاْتُنَّنِیْ بِهٖۤ اِلَّاۤ اَنْ یُّحَاطَ بِكُمْۚ-فَلَمَّاۤ اٰتَوْهُ مَوْثِقَهُمْ قَالَ اللّٰهُ عَلٰى مَا نَقُوْلُ وَكِیْلٌ(۶۶)', 0, 8, 2, 13, 3, 1, 12, 'قال لن أرسله معكم حتى تؤتون موثقا من الله لتأتنني به إلا أن يحاط بكم ۖ فلما آتوه موثقهم قال الله على ما نقول وكيل', 0),
(1674, 67, 'وَ قَالَ یٰبَنِیَّ لَا تَدْخُلُوْا مِنْۢ بَابٍ وَّاحِدٍ وَّ ادْخُلُوْا مِنْ اَبْوَابٍ مُّتَفَرِّقَةٍؕ-وَ مَاۤ اُغْنِیْ عَنْكُمْ مِّنَ اللّٰهِ مِنْ شَیْءٍؕ-اِنِ الْحُكْمُ اِلَّا لِلّٰهِؕ-عَلَیْهِ تَوَكَّلْتُۚ-وَ عَلَیْهِ فَلْیَتَوَكَّلِ الْمُتَوَكِّلُوْنَ(۶۷)', 0, 8, 2, 13, 3, 1, 12, 'وقال يا بني لا تدخلوا من باب واحد وادخلوا من أبواب متفرقة ۖ وما أغني عنكم من الله من شيء ۖ إن الحكم إلا لله ۖ عليه توكلت ۖ وعليه فليتوكل المتوكلون', 0),
(1675, 68, 'وَ لَمَّا دَخَلُوْا مِنْ حَیْثُ اَمَرَهُمْ اَبُوْهُمْؕ-مَا كَانَ یُغْنِیْ عَنْهُمْ مِّنَ اللّٰهِ مِنْ شَیْءٍ اِلَّا حَاجَةً فِیْ نَفْسِ یَعْقُوْبَ قَضٰىهَاؕ-وَ اِنَّهٗ لَذُوْ عِلْمٍ لِّمَا عَلَّمْنٰهُ وَ لٰكِنَّ اَكْثَرَ النَّاسِ لَا یَعْلَمُوْنَ۠(۶۸)', 0, 8, 2, 13, 3, 1, 12, 'ولما دخلوا من حيث أمرهم أبوهم ما كان يغني عنهم من الله من شيء إلا حاجة في نفس يعقوب قضاها ۚ وإنه لذو علم لما علمناه ولكن أكثر الناس لا يعلمون', 0),
(1676, 69, 'وَ لَمَّا دَخَلُوْا عَلٰى یُوْسُفَ اٰوٰۤى اِلَیْهِ اَخَاهُ قَالَ اِنِّیْۤ اَنَا اَخُوْكَ فَلَا تَبْتَىٕسْ بِمَا كَانُوْا یَعْمَلُوْنَ(۶۹) ', 0, 9, 3, 13, 3, 1, 12, 'ولما دخلوا على يوسف آوى إليه أخاه ۖ قال إني أنا أخوك فلا تبتئس بما كانوا يعملون', 0),
(1677, 70, 'فَلَمَّا جَهَّزَهُمْ بِجَهَازِهِمْ جَعَلَ السِّقَایَةَ فِیْ رَحْلِ اَخِیْهِ ثُمَّ اَذَّنَ مُؤَذِّنٌ اَیَّتُهَا الْعِیْرُ اِنَّكُمْ لَسٰرِقُوْنَ(۷۰)', 0, 9, 3, 13, 3, 1, 12, 'فلما جهزهم بجهازهم جعل السقاية في رحل أخيه ثم أذن مؤذن أيتها العير إنكم لسارقون', 0),
(1678, 71, 'قَالُوْا وَ اَقْبَلُوْا عَلَیْهِمْ مَّا ذَا تَفْقِدُوْنَ(۷۱)', 0, 9, 3, 13, 3, 1, 12, 'قالوا وأقبلوا عليهم ماذا تفقدون', 0),
(1679, 72, 'قَالُوْا نَفْقِدُ صُوَاعَ الْمَلِكِ وَ لِمَنْ جَآءَ بِهٖ حِمْلُ بَعِیْرٍ وَّ اَنَا بِهٖ زَعِیْمٌ(۷۲)', 0, 9, 3, 13, 3, 1, 12, 'قالوا نفقد صواع الملك ولمن جاء به حمل بعير وأنا به زعيم', 0),
(1680, 73, 'قَالُوْا تَاللّٰهِ لَقَدْ عَلِمْتُمْ مَّا جِئْنَا لِنُفْسِدَ فِی الْاَرْضِ وَ مَا كُنَّا سٰرِقِیْنَ(۷۳)', 0, 9, 3, 13, 3, 1, 12, 'قالوا تالله لقد علمتم ما جئنا لنفسد في الأرض وما كنا سارقين', 0),
(1681, 74, 'قَالُوْا فَمَا جَزَآؤُهٗۤ اِنْ كُنْتُمْ كٰذِبِیْنَ(۷۴)', 0, 9, 3, 13, 3, 1, 12, 'قالوا فما جزاؤه إن كنتم كاذبين', 0),
(1682, 75, 'قَالُوْا جَزَآؤُهٗ مَنْ وُّجِدَ فِیْ رَحْلِهٖ فَهُوَ جَزَآؤُهٗؕ-كَذٰلِكَ نَجْزِی الظّٰلِمِیْنَ(۷۵)', 0, 9, 3, 13, 3, 1, 12, 'قالوا جزاؤه من وجد في رحله فهو جزاؤه ۚ كذلك نجزي الظالمين', 0),
(1683, 76, 'فَبَدَاَ بِاَوْعِیَتِهِمْ قَبْلَ وِعَآءِ اَخِیْهِ ثُمَّ اسْتَخْرَجَهَا مِنْ وِّعَآءِ اَخِیْهِؕ-كَذٰلِكَ كِدْنَا لِیُوْسُفَؕ-مَا كَانَ لِیَاْخُذَ اَخَاهُ فِیْ دِیْنِ الْمَلِكِ اِلَّاۤ اَنْ یَّشَآءَ اللّٰهُؕ-نَرْفَعُ دَرَجٰتٍ مَّنْ نَّشَآءُؕ-وَ فَوْقَ كُلِّ ذِیْ عِلْمٍ عَلِیْمٌ(۷۶)', 0, 9, 3, 13, 3, 1, 12, 'فبدأ بأوعيتهم قبل وعاء أخيه ثم استخرجها من وعاء أخيه ۚ كذلك كدنا ليوسف ۖ ما كان ليأخذ أخاه في دين الملك إلا أن يشاء الله ۚ نرفع درجات من نشاء ۗ وفوق كل ذي علم عليم', 0),
(1684, 77, 'قَالُوْۤا اِنْ یَّسْرِقْ فَقَدْ سَرَقَ اَخٌ لَّهٗ مِنْ قَبْلُۚ-فَاَسَرَّهَا یُوْسُفُ فِیْ نَفْسِهٖ وَ لَمْ یُبْدِهَا لَهُمْۚ-قَالَ اَنْتُمْ شَرٌّ مَّكَانًاۚ-وَ اللّٰهُ اَعْلَمُ بِمَا تَصِفُوْنَ(۷۷)', 0, 9, 3, 13, 3, 1, 12, 'قالوا إن يسرق فقد سرق أخ له من قبل ۚ فأسرها يوسف في نفسه ولم يبدها لهم ۚ قال أنتم شر مكانا ۖ والله أعلم بما تصفون', 0),
(1685, 78, 'قَالُوْا یٰۤاَیُّهَا الْعَزِیْزُ اِنَّ لَهٗۤ اَبًا شَیْخًا كَبِیْرًا فَخُذْ اَحَدَنَا مَكَانَهٗۚ-اِنَّا نَرٰىكَ مِنَ الْمُحْسِنِیْنَ(۷۸)', 0, 9, 3, 13, 3, 1, 12, 'قالوا يا أيها العزيز إن له أبا شيخا كبيرا فخذ أحدنا مكانه ۖ إنا نراك من المحسنين', 0),
(1686, 79, 'قَالَ مَعَاذَ اللّٰهِ اَنْ نَّاْخُذَ اِلَّا مَنْ وَّجَدْنَا مَتَاعَنَا عِنْدَهٗۤۙ-اِنَّاۤ اِذًا لَّظٰلِمُوْنَ۠(۷۹)', 0, 9, 3, 13, 3, 1, 12, 'قال معاذ الله أن نأخذ إلا من وجدنا متاعنا عنده إنا إذا لظالمون', 0),
(1687, 80, 'فَلَمَّا اسْتَایْــٴَـسُوْا مِنْهُ خَلَصُوْا نَجِیًّاؕ-قَالَ كَبِیْرُهُمْ اَلَمْ تَعْلَمُوْۤا اَنَّ اَبَاكُمْ قَدْ اَخَذَ عَلَیْكُمْ مَّوْثِقًا مِّنَ اللّٰهِ وَ مِنْ قَبْلُ مَا فَرَّطْتُّمْ فِیْ یُوْسُفَۚ-فَلَنْ اَبْرَحَ الْاَرْضَ حَتّٰى یَاْذَنَ لِیْۤ اَبِیْۤ اَوْ یَحْكُمَ اللّٰهُ لِیْۚ-وَ هُوَ خَیْرُ الْحٰكِمِیْنَ(۸۰)', 0, 10, 4, 13, 3, 1, 12, 'فلما استيأسوا منه خلصوا نجيا ۖ قال كبيرهم ألم تعلموا أن أباكم قد أخذ عليكم موثقا من الله ومن قبل ما فرطتم في يوسف ۖ فلن أبرح الأرض حتى يأذن لي أبي أو يحكم الله لي ۖ وهو خير الحاكمين', 0),
(1688, 81, 'اِرْجِعُوْۤا اِلٰۤى اَبِیْكُمْ فَقُوْلُوْا یٰۤاَبَانَاۤ اِنَّ ابْنَكَ سَرَقَۚ-وَ مَا شَهِدْنَاۤ اِلَّا بِمَا عَلِمْنَا وَ مَا كُنَّا لِلْغَیْبِ حٰفِظِیْنَ(۸۱)', 0, 10, 4, 13, 3, 1, 12, 'ارجعوا إلى أبيكم فقولوا يا أبانا إن ابنك سرق وما شهدنا إلا بما علمنا وما كنا للغيب حافظين', 0),
(1689, 82, 'وَ سْــٴَـلِ الْقَرْیَةَ الَّتِیْ كُنَّا فِیْهَا وَ الْعِیْرَ الَّتِیْۤ اَقْبَلْنَا فِیْهَاؕ-وَ اِنَّا لَصٰدِقُوْنَ(۸۲)', 0, 10, 4, 13, 3, 1, 12, 'واسأل القرية التي كنا فيها والعير التي أقبلنا فيها ۖ وإنا لصادقون', 0);
INSERT INTO `quran` (`ayatId`, `ayatNumber`, `arabicText`, `ayatSajda`, `surahRuku`, `paraRuku`, `paraId`, `manzilNo`, `ayatVisible`, `surahId`, `withoutAerab`, `favourite`) VALUES
(1690, 83, 'قَالَ بَلْ سَوَّلَتْ لَكُمْ اَنْفُسُكُمْ اَمْرًاؕ-فَصَبْرٌ جَمِیْلٌؕ-عَسَى اللّٰهُ اَنْ یَّاْتِیَنِیْ بِهِمْ جَمِیْعًاؕ-اِنَّهٗ هُوَ الْعَلِیْمُ الْحَكِیْمُ(۸۳)', 0, 10, 4, 13, 3, 1, 12, 'قال بل سولت لكم أنفسكم أمرا ۖ فصبر جميل ۖ عسى الله أن يأتيني بهم جميعا ۚ إنه هو العليم الحكيم', 0),
(1691, 84, 'وَ تَوَلّٰى عَنْهُمْ وَ قَالَ یٰۤاَسَفٰى عَلٰى یُوْسُفَ وَ ابْیَضَّتْ عَیْنٰهُ مِنَ الْحُزْنِ فَهُوَ كَظِیْمٌ(۸۴)', 0, 10, 4, 13, 3, 1, 12, 'وتولى عنهم وقال يا أسفى على يوسف وابيضت عيناه من الحزن فهو كظيم', 0),
(1692, 85, 'قَالُوْا تَاللّٰهِ تَفْتَؤُا تَذْكُرُ یُوْسُفَ حَتّٰى تَكُوْنَ حَرَضًا اَوْ تَكُوْنَ مِنَ الْهٰلِكِیْنَ(۸۵)', 0, 10, 4, 13, 3, 1, 12, 'قالوا تالله تفتأ تذكر يوسف حتى تكون حرضا أو تكون من الهالكين', 0),
(1693, 86, 'قَالَ اِنَّمَاۤ اَشْكُوْا بَثِّیْ وَ حُزْنِیْۤ اِلَى اللّٰهِ وَ اَعْلَمُ مِنَ اللّٰهِ مَا لَا تَعْلَمُوْنَ(۸۶)', 0, 10, 4, 13, 3, 1, 12, 'قال إنما أشكو بثي وحزني إلى الله وأعلم من الله ما لا تعلمون', 0),
(1694, 87, 'یٰبَنِیَّ اذْهَبُوْا فَتَحَسَّسُوْا مِنْ یُّوْسُفَ وَ اَخِیْهِ وَ لَا تَایْــٴَـسُوْا مِنْ رَّوْحِ اللّٰهِؕ-اِنَّهٗ لَا یَایْــٴَـسُ مِنْ رَّوْحِ اللّٰهِ اِلَّا الْقَوْمُ الْكٰفِرُوْنَ(۸۷)', 0, 10, 4, 13, 3, 1, 12, 'يا بني اذهبوا فتحسسوا من يوسف وأخيه ولا تيأسوا من روح الله ۖ إنه لا ييأس من روح الله إلا القوم الكافرون', 0),
(1695, 88, 'فَلَمَّا دَخَلُوْا عَلَیْهِ قَالُوْا یٰۤاَیُّهَا الْعَزِیْزُ مَسَّنَا وَ اَهْلَنَا الضُّرُّ وَ جِئْنَا بِبِضَاعَةٍ مُّزْجٰىةٍ فَاَوْفِ لَنَا الْكَیْلَ وَ تَصَدَّقْ عَلَیْنَاؕ-اِنَّ اللّٰهَ یَجْزِی الْمُتَصَدِّقِیْنَ(۸۸)', 0, 10, 4, 13, 3, 1, 12, 'فلما دخلوا عليه قالوا يا أيها العزيز مسنا وأهلنا الضر وجئنا ببضاعة مزجاة فأوف لنا الكيل وتصدق علينا ۖ إن الله يجزي المتصدقين', 0),
(1696, 89, 'قَالَ هَلْ عَلِمْتُمْ مَّا فَعَلْتُمْ بِیُوْسُفَ وَ اَخِیْهِ اِذْ اَنْتُمْ جٰهِلُوْنَ(۸۹)', 0, 10, 4, 13, 3, 1, 12, 'قال هل علمتم ما فعلتم بيوسف وأخيه إذ أنتم جاهلون', 0),
(1697, 90, 'قَالُوْۤا ءَاِنَّكَ لَاَنْتَ یُوْسُفُؕ-قَالَ اَنَا یُوْسُفُ وَ هٰذَاۤ اَخِیْ٘-قَدْ مَنَّ اللّٰهُ عَلَیْنَاؕ-اِنَّهٗ مَنْ یَّتَّقِ وَ یَصْبِرْ فَاِنَّ اللّٰهَ لَا یُضِیْعُ اَجْرَ الْمُحْسِنِیْنَ(۹۰)', 0, 10, 4, 13, 3, 1, 12, 'قالوا أإنك لأنت يوسف ۖ قال أنا يوسف وهذا أخي ۖ قد من الله علينا ۖ إنه من يتق ويصبر فإن الله لا يضيع أجر المحسنين', 0),
(1698, 91, 'قَالُوْا تَاللّٰهِ لَقَدْ اٰثَرَكَ اللّٰهُ عَلَیْنَا وَ اِنْ كُنَّا لَخٰطِـٕیْنَ(۹۱)', 0, 10, 4, 13, 3, 1, 12, 'قالوا تالله لقد آثرك الله علينا وإن كنا لخاطئين', 0),
(1699, 92, 'قَالَ لَا تَثْرِیْبَ عَلَیْكُمُ الْیَوْمَؕ-یَغْفِرُ اللّٰهُ لَكُمْ٘-وَ هُوَ اَرْحَمُ الرّٰحِمِیْنَ(۹۲)', 0, 10, 4, 13, 3, 1, 12, 'قال لا تثريب عليكم اليوم ۖ يغفر الله لكم ۖ وهو أرحم الراحمين', 0),
(1700, 93, 'اِذْهَبُوْا بِقَمِیْصِیْ هٰذَا فَاَلْقُوْهُ عَلٰى وَجْهِ اَبِیْ یَاْتِ بَصِیْرًاۚ-وَ اْتُوْنِیْ بِاَهْلِكُمْ اَجْمَعِیْنَ۠(۹۳)', 0, 10, 4, 13, 3, 1, 12, 'اذهبوا بقميصي هذا فألقوه على وجه أبي يأت بصيرا وأتوني بأهلكم أجمعين', 0),
