(4394, 27, 'اِلَّا الَّذِیْ فَطَرَنِیْ فَاِنَّهٗ سَیَهْدِیْنِ(۲۷)', 0, 3, 9, 25, 6, 1, 43, 'إلا الذي فطرني فإنه سيهدين', 0),
(4395, 28, 'وَ جَعَلَهَا كَلِمَةًۢ بَاقِیَةً فِیْ عَقِبِهٖ لَعَلَّهُمْ یَرْجِعُوْنَ(۲۸)', 0, 3, 9, 25, 6, 1, 43, 'وجعلها كلمة باقية في عقبه لعلهم يرجعون', 0),
(4396, 29, 'بَلْ مَتَّعْتُ هٰۤؤُلَآءِ وَ اٰبَآءَهُمْ حَتّٰى جَآءَهُمُ الْحَقُّ وَ رَسُوْلٌ مُّبِیْنٌ(۲۹)', 0, 3, 9, 25, 6, 1, 43, 'بل متعت هؤلاء وآباءهم حتى جاءهم الحق ورسول مبين', 0),
(4397, 30, 'وَ لَمَّا جَآءَهُمُ الْحَقُّ قَالُوْا هٰذَا سِحْرٌ وَّ اِنَّا بِهٖ كٰفِرُوْنَ(۳۰)', 0, 3, 9, 25, 6, 1, 43, 'ولما جاءهم الحق قالوا هذا سحر وإنا به كافرون', 0);
INSERT INTO `quran` (`ayatId`, `ayatNumber`, `arabicText`, `ayatSajda`, `surahRuku`, `paraRuku`, `paraId`, `manzilNo`, `ayatVisible`, `surahId`, `withoutAerab`, `favourite`) VALUES
(4398, 31, 'وَ قَالُوْا لَوْ لَا نُزِّلَ هٰذَا الْقُرْاٰنُ عَلٰى رَجُلٍ مِّنَ الْقَرْیَتَیْنِ عَظِیْمٍ(۳۱)', 0, 3, 9, 25, 6, 1, 43, 'وقالوا لولا نزل هذا القرآن على رجل من القريتين عظيم', 0),
(4399, 32, 'اَهُمْ یَقْسِمُوْنَ رَحْمَتَ رَبِّكَؕ-نَحْنُ قَسَمْنَا بَیْنَهُمْ مَّعِیْشَتَهُمْ فِی الْحَیٰوةِ الدُّنْیَا وَ رَفَعْنَا بَعْضَهُمْ فَوْقَ بَعْضٍ دَرَجٰتٍ لِّیَتَّخِذَ بَعْضُهُمْ بَعْضًا سُخْرِیًّاؕ-وَ رَحْمَتُ رَبِّكَ خَیْرٌ مِّمَّا یَجْمَعُوْنَ(۳۲)', 0, 3, 9, 25, 6, 1, 43, 'أهم يقسمون رحمت ربك ۚ نحن قسمنا بينهم معيشتهم في الحياة الدنيا ۚ ورفعنا بعضهم فوق بعض درجات ليتخذ بعضهم بعضا سخريا ۗ ورحمت ربك خير مما يجمعون', 0),
(4400, 33, 'وَ لَوْ لَاۤ اَنْ یَّكُوْنَ النَّاسُ اُمَّةً وَّاحِدَةً لَّجَعَلْنَا لِمَنْ یَّكْفُرُ بِالرَّحْمٰنِ لِبُیُوْتِهِمْ سُقُفًا مِّنْ فِضَّةٍ وَّ مَعَارِجَ عَلَیْهَا یَظْهَرُوْنَۙ(۳۳)', 0, 3, 9, 25, 6, 1, 43, 'ولولا أن يكون الناس أمة واحدة لجعلنا لمن يكفر بالرحمن لبيوتهم سقفا من فضة ومعارج عليها يظهرون', 0),
(4401, 34, 'وَ لِبُیُوْتِهِمْ اَبْوَابًا وَّ سُرُرًا عَلَیْهَا یَتَّكِـُٔوْنَۙ(۳۴)', 0, 3, 9, 25, 6, 1, 43, 'ولبيوتهم أبوابا وسررا عليها يتكئون', 0),
(4402, 35, 'وَ زُخْرُفًاؕ-وَ اِنْ كُلُّ ذٰلِكَ لَمَّا مَتَاعُ الْحَیٰوةِ الدُّنْیَاؕ-وَ الْاٰخِرَةُ عِنْدَ رَبِّكَ لِلْمُتَّقِیْنَ۠(۳۵)', 0, 3, 9, 25, 6, 1, 43, 'وزخرفا ۚ وإن كل ذلك لما متاع الحياة الدنيا ۚ والآخرة عند ربك للمتقين', 0),
(4403, 36, 'وَ مَنْ یَّعْشُ عَنْ ذِكْرِ الرَّحْمٰنِ نُقَیِّضْ لَهٗ شَیْطٰنًا فَهُوَ لَهٗ قَرِیْنٌ(۳۶)', 0, 4, 10, 25, 6, 1, 43, 'ومن يعش عن ذكر الرحمن نقيض له شيطانا فهو له قرين', 0),
(4404, 37, 'وَ اِنَّهُمْ لَیَصُدُّوْنَهُمْ عَنِ السَّبِیْلِ وَ یَحْسَبُوْنَ اَنَّهُمْ مُّهْتَدُوْنَ(۳۷)', 0, 4, 10, 25, 6, 1, 43, 'وإنهم ليصدونهم عن السبيل ويحسبون أنهم مهتدون', 0),
(4405, 38, 'حَتّٰۤى اِذَا جَآءَنَا قَالَ یٰلَیْتَ بَیْنِیْ وَ بَیْنَكَ بُعْدَ الْمَشْرِقَیْنِ فَبِئْسَ الْقَرِیْنُ(۳۸)', 0, 4, 10, 25, 6, 1, 43, 'حتى إذا جاءنا قال يا ليت بيني وبينك بعد المشرقين فبئس القرين', 0),
(4406, 39, 'وَ لَنْ یَّنْفَعَكُمُ الْیَوْمَ اِذْ ظَّلَمْتُمْ اَنَّكُمْ فِی الْعَذَابِ مُشْتَرِكُوْنَ(۳۹)', 0, 4, 10, 25, 6, 1, 43, 'ولن ينفعكم اليوم إذ ظلمتم أنكم في العذاب مشتركون', 0),
(4407, 40, 'اَفَاَنْتَ تُسْمِــعُ الصُّمَّ اَوْ تَهْدِی الْعُمْیَ وَ مَنْ كَانَ فِیْ ضَلٰلٍ مُّبِیْنٍ(۴۰)', 0, 4, 10, 25, 6, 1, 43, 'أفأنت تسمع الصم أو تهدي العمي ومن كان في ضلال مبين', 0),
(4408, 41, 'فَاِمَّا نَذْهَبَنَّ بِكَ فَاِنَّا مِنْهُمْ مُّنْتَقِمُوْنَۙ(۴۱)', 0, 4, 10, 25, 6, 1, 43, 'فإما نذهبن بك فإنا منهم منتقمون', 0),
(4409, 42, 'اَوْ نُرِیَنَّكَ الَّذِیْ وَعَدْنٰهُمْ فَاِنَّا عَلَیْهِمْ مُّقْتَدِرُوْنَ(۴۲)', 0, 4, 10, 25, 6, 1, 43, 'أو نرينك الذي وعدناهم فإنا عليهم مقتدرون', 0),
(4410, 43, 'فَاسْتَمْسِكْ بِالَّذِیْۤ اُوْحِیَ اِلَیْكَۚ-اِنَّكَ عَلٰى صِرَاطٍ مُّسْتَقِیْمٍ(۴۳)', 0, 4, 10, 25, 6, 1, 43, 'فاستمسك بالذي أوحي إليك ۖ إنك على صراط مستقيم', 0),
(4411, 44, 'وَ اِنَّهٗ لَذِكْرٌ لَّكَ وَ لِقَوْمِكَۚ-وَ سَوْفَ تُسْــٴَـلُوْنَ(۴۴)', 0, 4, 10, 25, 6, 1, 43, 'وإنه لذكر لك ولقومك ۖ وسوف تسألون', 0),
(4412, 45, 'وَ سْــٴَـلْ مَنْ اَرْسَلْنَا مِنْ قَبْلِكَ مِنْ رُّسُلِنَاۤ اَجَعَلْنَا مِنْ دُوْنِ الرَّحْمٰنِ اٰلِهَةً یُّعْبَدُوْنَ۠(۴۵)', 0, 4, 10, 25, 6, 1, 43, 'واسأل من أرسلنا من قبلك من رسلنا أجعلنا من دون الرحمن آلهة يعبدون', 0),
(4413, 46, 'وَ لَقَدْ اَرْسَلْنَا مُوْسٰى بِاٰیٰتِنَاۤ اِلٰى فِرْعَوْنَ وَ مَلَاۡىٕهٖ فَقَالَ اِنِّیْ رَسُوْلُ رَبِّ الْعٰلَمِیْنَ(۴۶)', 0, 5, 11, 25, 6, 1, 43, 'ولقد أرسلنا موسى بآياتنا إلى فرعون وملئه فقال إني رسول رب العالمين', 0),
(4414, 47, 'فَلَمَّا جَآءَهُمْ بِاٰیٰتِنَاۤ اِذَا هُمْ مِّنْهَا یَضْحَكُوْنَ(۴۷)', 0, 5, 11, 25, 6, 1, 43, 'فلما جاءهم بآياتنا إذا هم منها يضحكون', 0),
(4415, 48, 'وَ مَا نُرِیْهِمْ مِّنْ اٰیَةٍ اِلَّا هِیَ اَكْبَرُ مِنْ اُخْتِهَا٘-وَ اَخَذْنٰهُمْ بِالْعَذَابِ لَعَلَّهُمْ یَرْجِعُوْنَ(۴۸)', 0, 5, 11, 25, 6, 1, 43, 'وما نريهم من آية إلا هي أكبر من أختها ۖ وأخذناهم بالعذاب لعلهم يرجعون', 0),
(4416, 49, 'وَ قَالُوْا یٰۤاَیُّهَ السّٰحِرُ ادْعُ لَنَا رَبَّكَ بِمَا عَهِدَ عِنْدَكَۚ-اِنَّنَا لَمُهْتَدُوْنَ(۴۹)', 0, 5, 11, 25, 6, 1, 43, 'وقالوا يا أيه الساحر ادع لنا ربك بما عهد عندك إننا لمهتدون', 0),
(4417, 50, 'فَلَمَّا كَشَفْنَا عَنْهُمُ الْعَذَابَ اِذَا هُمْ یَنْكُثُوْنَ(۵۰)', 0, 5, 11, 25, 6, 1, 43, 'فلما كشفنا عنهم العذاب إذا هم ينكثون', 0),
(4418, 51, 'وَ نَادٰى فِرْعَوْنُ فِیْ قَوْمِهٖ قَالَ یٰقَوْمِ اَلَیْسَ لِیْ مُلْكُ مِصْرَ وَ هٰذِهِ الْاَنْهٰرُ تَجْرِیْ مِنْ تَحْتِیْۚ-اَفَلَا تُبْصِرُوْنَؕ(۵۱)', 0, 5, 11, 25, 6, 1, 43, 'ونادى فرعون في قومه قال يا قوم أليس لي ملك مصر وهذه الأنهار تجري من تحتي ۖ أفلا تبصرون', 0),
(4419, 52, 'اَمْ اَنَا خَیْرٌ مِّنْ هٰذَا الَّذِیْ هُوَ مَهِیْنٌ ﳔ وَّ لَا یَكَادُ یُبِیْنُ(۵۲)', 0, 5, 11, 25, 6, 1, 43, 'أم أنا خير من هذا الذي هو مهين ولا يكاد يبين', 0),
(4420, 53, 'فَلَوْ لَاۤ اُلْقِیَ عَلَیْهِ اَسْوِرَةٌ مِّنْ ذَهَبٍ اَوْ جَآءَ مَعَهُ الْمَلٰٓىٕكَةُ مُقْتَرِنِیْنَ(۵۳)', 0, 5, 11, 25, 6, 1, 43, 'فلولا ألقي عليه أسورة من ذهب أو جاء معه الملائكة مقترنين', 0),
(4421, 54, 'فَاسْتَخَفَّ قَوْمَهٗ فَاَطَاعُوْهُؕ-اِنَّهُمْ كَانُوْا قَوْمًا فٰسِقِیْنَ(۵۴)', 0, 5, 11, 25, 6, 1, 43, 'فاستخف قومه فأطاعوه ۚ إنهم كانوا قوما فاسقين', 0),
(4422, 55, 'فَلَمَّاۤ اٰسَفُوْنَا انْتَقَمْنَا مِنْهُمْ فَاَغْرَقْنٰهُمْ اَجْمَعِیْنَۙ(۵۵)', 0, 5, 11, 25, 6, 1, 43, 'فلما آسفونا انتقمنا منهم فأغرقناهم أجمعين', 0),
(4423, 56, 'فَجَعَلْنٰهُمْ سَلَفًا وَّ مَثَلًا لِّلْاٰخِرِیْنَ۠(۵۶)', 0, 5, 11, 25, 6, 1, 43, 'فجعلناهم سلفا ومثلا للآخرين', 0),
(4424, 57, 'وَ لَمَّا ضُرِبَ ابْنُ مَرْیَمَ مَثَلًا اِذَا قَوْمُكَ مِنْهُ یَصِدُّوْنَ(۵۷)', 0, 6, 12, 25, 6, 1, 43, 'ولما ضرب ابن مريم مثلا إذا قومك منه يصدون', 0),
(4425, 58, 'وَ قَالُوْۤا ءَاٰلِهَتُنَا خَیْرٌ اَمْ هُوَؕ-مَا ضَرَبُوْهُ لَكَ اِلَّا جَدَلًاؕ-بَلْ هُمْ قَوْمٌ خَصِمُوْنَ(۵۸)', 0, 6, 12, 25, 6, 1, 43, 'وقالوا أآلهتنا خير أم هو ۚ ما ضربوه لك إلا جدلا ۚ بل هم قوم خصمون', 0),
(4426, 59, 'اِنْ هُوَ اِلَّا عَبْدٌ اَنْعَمْنَا عَلَیْهِ وَ جَعَلْنٰهُ مَثَلًا لِّبَنِیْۤ اِسْرَآءِیْلَؕ(۵۹)', 0, 6, 12, 25, 6, 1, 43, 'إن هو إلا عبد أنعمنا عليه وجعلناه مثلا لبني إسرائيل', 0),
(4427, 60, 'وَ لَوْ نَشَآءُ لَجَعَلْنَا مِنْكُمْ مَّلٰٓىٕكَةً فِی الْاَرْضِ یَخْلُفُوْنَ(۶۰)', 0, 6, 12, 25, 6, 1, 43, 'ولو نشاء لجعلنا منكم ملائكة في الأرض يخلفون', 0),
(4428, 61, 'وَ اِنَّهٗ لَعِلْمٌ لِّلسَّاعَةِ فَلَا تَمْتَرُنَّ بِهَا وَ اتَّبِعُوْنِؕ-هٰذَا صِرَاطٌ مُّسْتَقِیْمٌ(۶۱)', 0, 6, 12, 25, 6, 1, 43, 'وإنه لعلم للساعة فلا تمترن بها واتبعون ۚ هذا صراط مستقيم', 0),
(4429, 62, 'وَ لَا یَصُدَّنَّكُمُ الشَّیْطٰنُۚ-اِنَّهٗ لَكُمْ عَدُوٌّ مُّبِیْنٌ(۶۲)', 0, 6, 12, 25, 6, 1, 43, 'ولا يصدنكم الشيطان ۖ إنه لكم عدو مبين', 0),
(4430, 63, 'وَ لَمَّا جَآءَ عِیْسٰى بِالْبَیِّنٰتِ قَالَ قَدْ جِئْتُكُمْ بِالْحِكْمَةِ وَ لِاُبَیِّنَ لَكُمْ بَعْضَ الَّذِیْ تَخْتَلِفُوْنَ فِیْهِۚ-فَاتَّقُوا اللّٰهَ وَ اَطِیْعُوْنِ(۶۳)', 0, 6, 12, 25, 6, 1, 43, 'ولما جاء عيسى بالبينات قال قد جئتكم بالحكمة ولأبين لكم بعض الذي تختلفون فيه ۖ فاتقوا الله وأطيعون', 0),
(4431, 64, 'اِنَّ اللّٰهَ هُوَ رَبِّیْ وَ رَبُّكُمْ فَاعْبُدُوْهُؕ-هٰذَا صِرَاطٌ مُّسْتَقِیْمٌ(۶۴)', 0, 6, 12, 25, 6, 1, 43, 'إن الله هو ربي وربكم فاعبدوه ۚ هذا صراط مستقيم', 0),
(4432, 65, 'فَاخْتَلَفَ الْاَحْزَابُ مِنْۢ بَیْنِهِمْۚ-فَوَیْلٌ لِّلَّذِیْنَ ظَلَمُوْا مِنْ عَذَابِ یَوْمٍ اَلِیْمٍ(۶۵)', 0, 6, 12, 25, 6, 1, 43, 'فاختلف الأحزاب من بينهم ۖ فويل للذين ظلموا من عذاب يوم أليم', 0),
(4433, 66, 'هَلْ یَنْظُرُوْنَ اِلَّا السَّاعَةَ اَنْ تَاْتِیَهُمْ بَغْتَةً وَّ هُمْ لَا یَشْعُرُوْنَ(۶۶)', 0, 6, 12, 25, 6, 1, 43, 'هل ينظرون إلا الساعة أن تأتيهم بغتة وهم لا يشعرون', 0),
(4434, 67, 'اَلْاَخِلَّآءُ یَوْمَىٕذٍۭ بَعْضُهُمْ لِبَعْضٍ عَدُوٌّ اِلَّا الْمُتَّقِیْنَؕ۠(۶۷)', 0, 6, 12, 25, 6, 1, 43, 'الأخلاء يومئذ بعضهم لبعض عدو إلا المتقين', 0),
(4435, 68, 'یٰعِبَادِ لَا خَوْفٌ عَلَیْكُمُ الْیَوْمَ وَ لَاۤ اَنْتُمْ تَحْزَنُوْنَۚ(۶۸)', 0, 7, 13, 25, 6, 1, 43, 'يا عباد لا خوف عليكم اليوم ولا أنتم تحزنون', 0),
(4436, 69, 'اَلَّذِیْنَ اٰمَنُوْا بِاٰیٰتِنَا وَ كَانُوْا مُسْلِمِیْنَۚ(۶۹)', 0, 7, 13, 25, 6, 1, 43, 'الذين آمنوا بآياتنا وكانوا مسلمين', 0),
(4437, 70, 'اُدْخُلُوا الْجَنَّةَ اَنْتُمْ وَ اَزْوَاجُكُمْ تُحْبَرُوْنَ(۷۰)', 0, 7, 13, 25, 6, 1, 43, 'ادخلوا الجنة أنتم وأزواجكم تحبرون', 0),
