(2552, 49, 'الَّذِیْنَ یَخْشَوْنَ رَبَّهُمْ بِالْغَیْبِ وَ هُمْ مِّنَ السَّاعَةِ مُشْفِقُوْنَ(۴۹)', 0, 4, 4, 17, 4, 1, 21, 'الذين يخشون ربهم بالغيب وهم من الساعة مشفقون', 0),
(2553, 50, 'وَ هٰذَا ذِكْرٌ مُّبٰرَكٌ اَنْزَلْنٰهُؕ-اَفَاَنْتُمْ لَهٗ مُنْكِرُوْنَ۠(۵۰)', 0, 4, 4, 17, 4, 1, 21, 'وهذا ذكر مبارك أنزلناه ۚ أفأنتم له منكرون', 0),
(2554, 51, 'وَ لَقَدْ اٰتَیْنَاۤ اِبْرٰهِیْمَ رُشْدَهٗ مِنْ قَبْلُ وَ كُنَّا بِهٖ عٰلِمِیْنَۚ(۵۱)', 0, 5, 5, 17, 4, 1, 21, 'ولقد آتينا إبراهيم رشده من قبل وكنا به عالمين', 0),
(2555, 52, 'اِذْ قَالَ لِاَبِیْهِ وَ قَوْمِهٖ مَا هٰذِهِ التَّمَاثِیْلُ الَّتِیْۤ اَنْتُمْ لَهَا عٰكِفُوْنَ(۵۲)', 0, 5, 5, 17, 4, 1, 21, 'إذ قال لأبيه وقومه ما هذه التماثيل التي أنتم لها عاكفون', 0),
(2556, 53, 'قَالُوْا وَجَدْنَاۤ اٰبَآءَنَا لَهَا عٰبِدِیْنَ(۵۳)', 0, 5, 5, 17, 4, 1, 21, 'قالوا وجدنا آباءنا لها عابدين', 0),
(2557, 54, 'قَالَ لَقَدْ كُنْتُمْ اَنْتُمْ وَ اٰبَآؤُكُمْ فِیْ ضَلٰلٍ مُّبِیْنٍ(۵۴)', 0, 5, 5, 17, 4, 1, 21, 'قال لقد كنتم أنتم وآباؤكم في ضلال مبين', 0),
(2558, 55, 'قَالُوْۤا اَجِئْتَنَا بِالْحَقِّ اَمْ اَنْتَ مِنَ اللّٰعِبِیْنَ(۵۵)', 0, 5, 5, 17, 4, 1, 21, 'قالوا أجئتنا بالحق أم أنت من اللاعبين', 0),
(2559, 56, 'قَالَ بَلْ رَّبُّكُمْ رَبُّ السَّمٰوٰتِ وَ الْاَرْضِ الَّذِیْ فَطَرَهُنَّ ﳲ وَ اَنَا عَلٰى ذٰلِكُمْ مِّنَ الشّٰهِدِیْنَ(۵۶)', 0, 5, 5, 17, 4, 1, 21, 'قال بل ربكم رب السماوات والأرض الذي فطرهن وأنا على ذلكم من الشاهدين', 0),
(2560, 57, 'وَ تَاللّٰهِ لَاَكِیْدَنَّ اَصْنَامَكُمْ بَعْدَ اَنْ تُوَلُّوْا مُدْبِرِیْنَ(۵۷)', 0, 5, 5, 17, 4, 1, 21, 'وتالله لأكيدن أصنامكم بعد أن تولوا مدبرين', 0),
(2561, 58, 'فَجَعَلَهُمْ جُذٰذًا اِلَّا كَبِیْرًا لَّهُمْ لَعَلَّهُمْ اِلَیْهِ یَرْجِعُوْنَ(۵۸)', 0, 5, 5, 17, 4, 1, 21, 'فجعلهم جذاذا إلا كبيرا لهم لعلهم إليه يرجعون', 0),
(2562, 59, 'قَالُوْا مَنْ فَعَلَ هٰذَا بِاٰلِهَتِنَاۤ اِنَّهٗ لَمِنَ الظّٰلِمِیْنَ(۵۹)', 0, 5, 5, 17, 4, 1, 21, 'قالوا من فعل هذا بآلهتنا إنه لمن الظالمين', 0),
(2563, 60, 'قَالُوْا سَمِعْنَا فَتًى یَّذْكُرُهُمْ یُقَالُ لَهٗۤ اِبْرٰهِیْمُؕ(۶۰)', 0, 5, 5, 17, 4, 1, 21, 'قالوا سمعنا فتى يذكرهم يقال له إبراهيم', 0),
(2564, 61, 'قَالُوْا فَاْتُوْا بِهٖ عَلٰۤى اَعْیُنِ النَّاسِ لَعَلَّهُمْ یَشْهَدُوْنَ(۶۱)', 0, 5, 5, 17, 4, 1, 21, 'قالوا فأتوا به على أعين الناس لعلهم يشهدون', 0),
(2565, 62, 'قَالُوْۤا ءَاَنْتَ فَعَلْتَ هٰذَا بِاٰلِهَتِنَا یٰۤاِبْرٰهِیْمُؕ(۶۲)', 0, 5, 5, 17, 4, 1, 21, 'قالوا أأنت فعلت هذا بآلهتنا يا إبراهيم', 0),
(2566, 63, 'قَالَ بَلْ فَعَلَهٗ ﳓ كَبِیْرُهُمْ هٰذَا فَسْــٴَـلُوْهُمْ اِنْ كَانُوْا یَنْطِقُوْنَ(۶۳)', 0, 5, 5, 17, 4, 1, 21, 'قال بل فعله كبيرهم هذا فاسألوهم إن كانوا ينطقون', 0),
(2567, 64, 'فَرَجَعُوْۤا اِلٰۤى اَنْفُسِهِمْ فَقَالُوْۤا اِنَّكُمْ اَنْتُمُ الظّٰلِمُوْنَۙ(۶۴)', 0, 5, 5, 17, 4, 1, 21, 'فرجعوا إلى أنفسهم فقالوا إنكم أنتم الظالمون', 0),
(2568, 65, 'ثُمَّ نُكِسُوْا عَلٰى رُءُوْسِهِمْۚ-لَقَدْ عَلِمْتَ مَا هٰۤؤُلَآءِ یَنْطِقُوْنَ(۶۵)', 0, 5, 5, 17, 4, 1, 21, 'ثم نكسوا على رءوسهم لقد علمت ما هؤلاء ينطقون', 0),
(2569, 66, 'قَالَ اَفَتَعْبُدُوْنَ مِنْ دُوْنِ اللّٰهِ مَا لَا یَنْفَعُكُمْ شَیْــٴًـا وَّ لَا یَضُرُّكُمْؕ(۶۶)', 0, 5, 5, 17, 4, 1, 21, 'قال أفتعبدون من دون الله ما لا ينفعكم شيئا ولا يضركم', 0),
(2570, 67, 'اُفٍّ لَّكُمْ وَ لِمَا تَعْبُدُوْنَ مِنْ دُوْنِ اللّٰهِؕ-اَفَلَا تَعْقِلُوْنَ(۶۷)', 0, 5, 5, 17, 4, 1, 21, 'أف لكم ولما تعبدون من دون الله ۖ أفلا تعقلون', 0),
(2571, 68, 'قَالُوْا حَرِّقُوْهُ وَ انْصُرُوْۤا اٰلِهَتَكُمْ اِنْ كُنْتُمْ فٰعِلِیْنَ(۶۸)', 0, 5, 5, 17, 4, 1, 21, 'قالوا حرقوه وانصروا آلهتكم إن كنتم فاعلين', 0),
(2572, 69, 'قُلْنَا یٰنَارُ كُوْنِیْ بَرْدًا وَّ سَلٰمًا عَلٰۤى اِبْرٰهِیْمَۙ(۶۹)', 0, 5, 5, 17, 4, 1, 21, 'قلنا يا نار كوني بردا وسلاما على إبراهيم', 0),
(2573, 70, 'وَ اَرَادُوْا بِهٖ كَیْدًا فَجَعَلْنٰهُمُ الْاَخْسَرِیْنَۚ(۷۰)', 0, 5, 5, 17, 4, 1, 21, 'وأرادوا به كيدا فجعلناهم الأخسرين', 0),
(2574, 71, 'وَ نَجَّیْنٰهُ وَ لُوْطًا اِلَى الْاَرْضِ الَّتِیْ بٰرَكْنَا فِیْهَا لِلْعٰلَمِیْنَ(۷۱)', 0, 5, 5, 17, 4, 1, 21, 'ونجيناه ولوطا إلى الأرض التي باركنا فيها للعالمين', 0),
(2575, 72, 'وَ وَهَبْنَا لَهٗۤ اِسْحٰقَؕ-وَ یَعْقُوْبَ نَافِلَةًؕ-وَ كُلًّا جَعَلْنَا صٰلِحِیْنَ(۷۲)', 0, 5, 5, 17, 4, 1, 21, 'ووهبنا له إسحاق ويعقوب نافلة ۖ وكلا جعلنا صالحين', 0);
INSERT INTO `quran` (`ayatId`, `ayatNumber`, `arabicText`, `ayatSajda`, `surahRuku`, `paraRuku`, `paraId`, `manzilNo`, `ayatVisible`, `surahId`, `withoutAerab`, `favourite`) VALUES
(2576, 73, 'وَ جَعَلْنٰهُمْ اَىٕمَّةً یَّهْدُوْنَ بِاَمْرِنَا وَ اَوْحَیْنَاۤ اِلَیْهِمْ فِعْلَ الْخَیْرٰتِ وَ اِقَامَ الصَّلٰوةِ وَ اِیْتَآءَ الزَّكٰوةِۚ-وَ كَانُوْا لَنَا عٰبِدِیْنَۚۙ(۷۳)', 0, 5, 5, 17, 4, 1, 21, 'وجعلناهم أئمة يهدون بأمرنا وأوحينا إليهم فعل الخيرات وإقام الصلاة وإيتاء الزكاة ۖ وكانوا لنا عابدين', 0),
(2577, 74, 'وَ لُوْطًا اٰتَیْنٰهُ حُكْمًا وَّ عِلْمًا وَّ نَجَّیْنٰهُ مِنَ الْقَرْیَةِ الَّتِیْ كَانَتْ تَّعْمَلُ الْخَبٰٓىٕثَؕ-اِنَّهُمْ كَانُوْا قَوْمَ سَوْءٍ فٰسِقِیْنَۙ(۷۴)', 0, 5, 5, 17, 4, 1, 21, 'ولوطا آتيناه حكما وعلما ونجيناه من القرية التي كانت تعمل الخبائث ۗ إنهم كانوا قوم سوء فاسقين', 0),
(2578, 75, 'وَ اَدْخَلْنٰهُ فِیْ رَحْمَتِنَاؕ-اِنَّهٗ مِنَ الصّٰلِحِیْنَ۠(۷۵)', 0, 5, 5, 17, 4, 1, 21, 'وأدخلناه في رحمتنا ۖ إنه من الصالحين', 0),
(2579, 76, 'وَ نُوْحًا اِذْ نَادٰى مِنْ قَبْلُ فَاسْتَجَبْنَا لَهٗ فَنَجَّیْنٰهُ وَ اَهْلَهٗ مِنَ الْكَرْبِ الْعَظِیْمِۚ(۷۶)', 0, 6, 6, 17, 4, 1, 21, 'ونوحا إذ نادى من قبل فاستجبنا له فنجيناه وأهله من الكرب العظيم', 0),
(2580, 77, 'وَ نَصَرْنٰهُ مِنَ الْقَوْمِ الَّذِیْنَ كَذَّبُوْا بِاٰیٰتِنَاؕ-اِنَّهُمْ كَانُوْا قَوْمَ سَوْءٍ فَاَغْرَقْنٰهُمْ اَجْمَعِیْنَ(۷۷)', 0, 6, 6, 17, 4, 1, 21, 'ونصرناه من القوم الذين كذبوا بآياتنا ۚ إنهم كانوا قوم سوء فأغرقناهم أجمعين', 0),
(2581, 78, 'وَ دَاوٗدَ وَ سُلَیْمٰنَ اِذْ یَحْكُمٰنِ فِی الْحَرْثِ اِذْ نَفَشَتْ فِیْهِ غَنَمُ الْقَوْمِۚ-وَ كُنَّا لِحُكْمِهِمْ شٰهِدِیْنَۗۙ(۷۸)', 0, 6, 6, 17, 4, 1, 21, 'وداوود وسليمان إذ يحكمان في الحرث إذ نفشت فيه غنم القوم وكنا لحكمهم شاهدين', 0),
(2582, 79, 'فَفَهَّمْنٰهَا سُلَیْمٰنَۚ-وَ كُلًّا اٰتَیْنَا حُكْمًا وَّ عِلْمًا٘-وَّ سَخَّرْنَا مَعَ دَاوٗدَ الْجِبَالَ یُسَبِّحْنَ وَ الطَّیْرَؕ-وَ كُنَّا فٰعِلِیْنَ(۷۹)', 0, 6, 6, 17, 4, 1, 21, 'ففهمناها سليمان ۚ وكلا آتينا حكما وعلما ۚ وسخرنا مع داوود الجبال يسبحن والطير ۚ وكنا فاعلين', 0),
(2583, 80, 'وَ عَلَّمْنٰهُ صَنْعَةَ لَبُوْسٍ لَّكُمْ لِتُحْصِنَكُمْ مِّنْۢ بَاْسِكُمْۚ-فَهَلْ اَنْتُمْ شٰكِرُوْنَ(۸۰)', 0, 6, 6, 17, 4, 1, 21, 'وعلمناه صنعة لبوس لكم لتحصنكم من بأسكم ۖ فهل أنتم شاكرون', 0),
(2584, 81, 'وَ لِسُلَیْمٰنَ الرِّیْحَ عَاصِفَةً تَجْرِیْ بِاَمْرِهٖۤ اِلَى الْاَرْضِ الَّتِیْ بٰرَكْنَا فِیْهَاؕ-وَ كُنَّا بِكُلِّ شَیْءٍ عٰلِمِیْنَ(۸۱)', 0, 6, 6, 17, 4, 1, 21, 'ولسليمان الريح عاصفة تجري بأمره إلى الأرض التي باركنا فيها ۚ وكنا بكل شيء عالمين', 0),
(2585, 82, 'وَ مِنَ الشَّیٰطِیْنِ مَنْ یَّغُوْصُوْنَ لَهٗ وَ یَعْمَلُوْنَ عَمَلًا دُوْنَ ذٰلِكَۚ-وَ كُنَّا لَهُمْ حٰفِظِیْنَۙ(۸۲)', 0, 6, 6, 17, 4, 1, 21, 'ومن الشياطين من يغوصون له ويعملون عملا دون ذلك ۖ وكنا لهم حافظين', 0),
(2586, 83, 'وَ اَیُّوْبَ اِذْ نَادٰى رَبَّهٗۤ اَنِّیْ مَسَّنِیَ الضُّرُّ وَ اَنْتَ اَرْحَمُ الرّٰحِمِیْنَۚۖ(۸۳)', 0, 6, 6, 17, 4, 1, 21, 'وأيوب إذ نادى ربه أني مسني الضر وأنت أرحم الراحمين', 0),
(2587, 84, 'فَاسْتَجَبْنَا لَهٗ فَكَشَفْنَا مَا بِهٖ مِنْ ضُرٍّ وَّ اٰتَیْنٰهُ اَهْلَهٗ وَ مِثْلَهُمْ مَّعَهُمْ رَحْمَةً مِّنْ عِنْدِنَا وَ ذِكْرٰى لِلْعٰبِدِیْنَ(۸۴)', 0, 6, 6, 17, 4, 1, 21, 'فاستجبنا له فكشفنا ما به من ضر ۖ وآتيناه أهله ومثلهم معهم رحمة من عندنا وذكرى للعابدين', 0),
(2588, 85, 'وَ اِسْمٰعِیْلَ وَ اِدْرِیْسَ وَ ذَا الْكِفْلِؕ-كُلٌّ مِّنَ الصّٰبِرِیْنَۚۖ(۸۵)', 0, 6, 6, 17, 4, 1, 21, 'وإسماعيل وإدريس وذا الكفل ۖ كل من الصابرين', 0),
(2589, 86, 'وَ اَدْخَلْنٰهُمْ فِیْ رَحْمَتِنَاؕ-اِنَّهُمْ مِّنَ الصّٰلِحِیْنَ(۸۶)', 0, 6, 6, 17, 4, 1, 21, 'وأدخلناهم في رحمتنا ۖ إنهم من الصالحين', 0),
(2590, 87, 'وَ ذَا النُّوْنِ اِذْ ذَّهَبَ مُغَاضِبًا فَظَنَّ اَنْ لَّنْ نَّقْدِرَ عَلَیْهِ فَنَادٰى فِی الظُّلُمٰتِ اَنْ لَّاۤ اِلٰهَ اِلَّاۤ اَنْتَ سُبْحٰنَكَ ﳓ اِنِّیْ كُنْتُ مِنَ الظّٰلِمِیْنَۚۖ(۸۷)', 0, 6, 6, 17, 4, 1, 21, 'وذا النون إذ ذهب مغاضبا فظن أن لن نقدر عليه فنادى في الظلمات أن لا إله إلا أنت سبحانك إني كنت من الظالمين', 0),
(2591, 88, 'فَاسْتَجَبْنَا لَهٗۙ-وَ نَجَّیْنٰهُ مِنَ الْغَمِّؕ-وَ كَذٰلِكَ نُـْۨجِی الْمُؤْمِنِیْنَ(۸۸)', 0, 6, 6, 17, 4, 1, 21, 'فاستجبنا له ونجيناه من الغم ۚ وكذلك ننجي المؤمنين', 0),
(2592, 89, 'وَ زَكَرِیَّاۤ اِذْ نَادٰى رَبَّهٗ رَبِّ لَا تَذَرْنِیْ فَرْدًا وَّ اَنْتَ خَیْرُ الْوٰرِثِیْنَۚۖ(۸۹)', 0, 6, 6, 17, 4, 1, 21, 'وزكريا إذ نادى ربه رب لا تذرني فردا وأنت خير الوارثين', 0),
(2593, 90, 'فَاسْتَجَبْنَا لَهٗ٘-وَ وَهَبْنَا لَهٗ یَحْیٰى وَ اَصْلَحْنَا لَهٗ زَوْجَهٗؕ-اِنَّهُمْ كَانُوْا یُسٰرِعُوْنَ فِی الْخَیْرٰتِ وَ یَدْعُوْنَنَا رَغَبًا وَّ رَهَبًاؕ-وَ كَانُوْا لَنَا خٰشِعِیْنَ(۹۰)', 0, 6, 6, 17, 4, 1, 21, 'فاستجبنا له ووهبنا له يحيى وأصلحنا له زوجه ۚ إنهم كانوا يسارعون في الخيرات ويدعوننا رغبا ورهبا ۖ وكانوا لنا خاشعين', 0),
(2594, 91, 'وَ الَّتِیْۤ اَحْصَنَتْ فَرْجَهَا فَنَفَخْنَا فِیْهَا مِنْ رُّوْحِنَا وَ جَعَلْنٰهَا وَ ابْنَهَاۤ اٰیَةً لِّلْعٰلَمِیْنَ(۹۱)', 0, 6, 6, 17, 4, 1, 21, 'والتي أحصنت فرجها فنفخنا فيها من روحنا وجعلناها وابنها آية للعالمين', 0),
