(4902, 3, 'وَ كَذَّبُوْا وَ اتَّبَعُوْۤا اَهْوَآءَهُمْ وَ كُلُّ اَمْرٍ مُّسْتَقِرٌّ(۳)', 0, 1, 8, 27, 7, 1, 54, 'وكذبوا واتبعوا أهواءهم ۚ وكل أمر مستقر', 0),
(4903, 4, 'وَ لَقَدْ جَآءَهُمْ مِّنَ الْاَنْۢبَآءِ مَا فِیْهِ مُزْدَجَرٌۙ(۴)', 0, 1, 8, 27, 7, 1, 54, 'ولقد جاءهم من الأنباء ما فيه مزدجر', 0),
(4904, 5, 'حِكْمَةٌۢ بَالِغَةٌ فَمَا تُغْنِ النُّذُرُۙ(۵)', 0, 1, 8, 27, 7, 1, 54, 'حكمة بالغة ۖ فما تغن النذر', 0),
(4905, 6, 'فَتَوَلَّ عَنْهُمْۘ- یَوْمَ یَدْعُ الدَّاعِ اِلٰى شَیْءٍ نُّكُرٍۙ(۶)', 0, 1, 8, 27, 7, 1, 54, 'فتول عنهم ۘ يوم يدع الداع إلى شيء نكر', 0),
(4906, 7, 'خُشَّعًا اَبْصَارُهُمْ یَخْرُجُوْنَ مِنَ الْاَجْدَاثِ كَاَنَّهُمْ جَرَادٌ مُّنْتَشِرٌۙ(۷)', 0, 1, 8, 27, 7, 1, 54, 'خشعا أبصارهم يخرجون من الأجداث كأنهم جراد منتشر', 0),
(4907, 8, 'مُّهْطِعِیْنَ اِلَى الدَّاعِؕ- یَقُوْلُ الْكٰفِرُوْنَ هٰذَا یَوْمٌ عَسِرٌ(۸)', 0, 1, 8, 27, 7, 1, 54, 'مهطعين إلى الداع ۖ يقول الكافرون هذا يوم عسر', 0),
(4908, 9, 'كَذَّبَتْ قَبْلَهُمْ قَوْمُ نُوْحٍ فَكَذَّبُوْا عَبْدَنَا وَ قَالُوْا مَجْنُوْنٌ وَّ ازْدُجِرَ(۹)', 0, 1, 8, 27, 7, 1, 54, 'كذبت قبلهم قوم نوح فكذبوا عبدنا وقالوا مجنون وازدجر', 0),
(4909, 10, 'فَدَعَا رَبَّهٗۤ اَنِّیْ مَغْلُوْبٌ فَانْتَصِرْ(۱۰)', 0, 1, 8, 27, 7, 1, 54, 'فدعا ربه أني مغلوب فانتصر', 0),
(4910, 11, 'فَفَتَحْنَاۤ اَبْوَابَ السَّمَآءِ بِمَآءٍ مُّنْهَمِرٍ٘ۖ(۱۱)', 0, 1, 8, 27, 7, 1, 54, 'ففتحنا أبواب السماء بماء منهمر', 0),
(4911, 12, 'وَّ فَجَّرْنَا الْاَرْضَ عُیُوْنًا فَالْتَقَى الْمَآءُ عَلٰۤى اَمْرٍ قَدْ قُدِرَۚ(۱۲)', 0, 1, 8, 27, 7, 1, 54, 'وفجرنا الأرض عيونا فالتقى الماء على أمر قد قدر', 0),
(4912, 13, 'وَ حَمَلْنٰهُ عَلٰى ذَاتِ اَلْوَاحٍ وَّ دُسُرٍۙ(۱۳)', 0, 1, 8, 27, 7, 1, 54, 'وحملناه على ذات ألواح ودسر', 0),
(4913, 14, 'تَجْرِیْ بِاَعْیُنِنَاۚ-جَزَآءً لِّمَنْ كَانَ كُفِرَ(۱۴)', 0, 1, 8, 27, 7, 1, 54, 'تجري بأعيننا جزاء لمن كان كفر', 0),
(4914, 15, 'وَ لَقَدْ تَّرَكْنٰهَاۤ اٰیَةً فَهَلْ مِنْ مُّدَّكِرٍ(۱۵)', 0, 1, 8, 27, 7, 1, 54, 'ولقد تركناها آية فهل من مدكر', 0),
(4915, 16, 'فَكَیْفَ كَانَ عَذَابِیْ وَ نُذُرِ(۱۶)', 0, 1, 8, 27, 7, 1, 54, 'فكيف كان عذابي ونذر', 0),
(4916, 17, 'وَ لَقَدْ یَسَّرْنَا الْقُرْاٰنَ لِلذِّكْرِ فَهَلْ مِنْ مُّدَّكِرٍ(۱۷)', 0, 1, 8, 27, 7, 1, 54, 'ولقد يسرنا القرآن للذكر فهل من مدكر', 0),
(4917, 18, 'كَذَّبَتْ عَادٌ فَكَیْفَ كَانَ عَذَابِیْ وَ نُذُرِ(۱۸)', 0, 1, 8, 27, 7, 1, 54, 'كذبت عاد فكيف كان عذابي ونذر', 0),
(4918, 19, 'اِنَّاۤ اَرْسَلْنَا عَلَیْهِمْ رِیْحًا صَرْصَرًا فِیْ یَوْمِ نَحْسٍ مُّسْتَمِرٍّۙ(۱۹)', 0, 1, 8, 27, 7, 1, 54, 'إنا أرسلنا عليهم ريحا صرصرا في يوم نحس مستمر', 0),
(4919, 20, 'تَنْزِعُ النَّاسَۙ- كَاَنَّهُمْ اَعْجَازُ نَخْلٍ مُّنْقَعِرٍ(۲۰)', 0, 1, 8, 27, 7, 1, 54, 'تنزع الناس كأنهم أعجاز نخل منقعر', 0),
(4920, 21, 'فَكَیْفَ كَانَ عَذَابِیْ وَ نُذُرِ(۲۱)', 0, 1, 8, 27, 7, 1, 54, 'فكيف كان عذابي ونذر', 0),
(4921, 22, 'وَ لَقَدْ یَسَّرْنَا الْقُرْاٰنَ لِلذِّكْرِ فَهَلْ مِنْ مُّدَّكِرٍ۠(۲۲)', 0, 1, 8, 27, 7, 1, 54, 'ولقد يسرنا القرآن للذكر فهل من مدكر', 0),
(4922, 23, 'كَذَّبَتْ ثَمُوْدُ بِالنُّذُرِ(۲۳)', 0, 2, 9, 27, 7, 1, 54, 'كذبت ثمود بالنذر', 0);
