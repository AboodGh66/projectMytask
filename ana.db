(2771, 76, 'وَ لَقَدْ اَخَذْنٰهُمْ بِالْعَذَابِ فَمَا اسْتَكَانُوْا لِرَبِّهِمْ وَ مَا یَتَضَرَّعُوْنَ(۷۶)', 0, 4, 4, 18, 4, 1, 23, 'ولقد أخذناهم بالعذاب فما استكانوا لربهم وما يتضرعون', 0),
(2772, 77, 'حَتّٰۤى اِذَا فَتَحْنَا عَلَیْهِمْ بَابًا ذَا عَذَابٍ شَدِیْدٍ اِذَا هُمْ فِیْهِ مُبْلِسُوْنَ۠(۷۷)', 0, 4, 4, 18, 4, 1, 23, 'حتى إذا فتحنا عليهم بابا ذا عذاب شديد إذا هم فيه مبلسون', 0),
(2773, 78, 'وَ هُوَ الَّذِیْۤ اَنْشَاَ لَكُمُ السَّمْعَ وَ الْاَبْصَارَ وَ الْاَفْـٕدَةَؕ-قَلِیْلًا مَّا تَشْكُرُوْنَ(۷۸)', 0, 5, 5, 18, 4, 1, 23, 'وهو الذي أنشأ لكم السمع والأبصار والأفئدة ۚ قليلا ما تشكرون', 0),
(2774, 79, 'وَ هُوَ الَّذِیْ ذَرَاَكُمْ فِی الْاَرْضِ وَ اِلَیْهِ تُحْشَرُوْنَ(۷۹)', 0, 5, 5, 18, 4, 1, 23, 'وهو الذي ذرأكم في الأرض وإليه تحشرون', 0),
(2775, 80, 'وَ هُوَ الَّذِیْ یُحْیٖ وَ یُمِیْتُ وَ لَهُ اخْتِلَافُ الَّیْلِ وَ النَّهَارِؕ-اَفَلَا تَعْقِلُوْنَ(۸۰)', 0, 5, 5, 18, 4, 1, 23, 'وهو الذي يحيي ويميت وله اختلاف الليل والنهار ۚ أفلا تعقلون', 0),
(2776, 81, 'بَلْ قَالُوْا مِثْلَ مَا قَالَ الْاَوَّلُوْنَ(۸۱)', 0, 5, 5, 18, 4, 1, 23, 'بل قالوا مثل ما قال الأولون', 0),
(2777, 82, 'قَالُوْۤا ءَاِذَا مِتْنَا وَ كُنَّا تُرَابًا وَّ عِظَامًا ءَاِنَّا لَمَبْعُوْثُوْنَ(۸۲)', 0, 5, 5, 18, 4, 1, 23, 'قالوا أإذا متنا وكنا ترابا وعظاما أإنا لمبعوثون', 0),
(2778, 83, 'لَقَدْ وُعِدْنَا نَحْنُ وَ اٰبَآؤُنَا هٰذَا مِنْ قَبْلُ اِنْ هٰذَاۤ اِلَّاۤ اَسَاطِیْرُ الْاَوَّلِیْنَ(۸۳)', 0, 5, 5, 18, 4, 1, 23, 'لقد وعدنا نحن وآباؤنا هذا من قبل إن هذا إلا أساطير الأولين', 0),
(2779, 84, 'قُلْ لِّمَنِ الْاَرْضُ وَ مَنْ فِیْهَاۤ اِنْ كُنْتُمْ تَعْلَمُوْنَ(۸۴)', 0, 5, 5, 18, 4, 1, 23, 'قل لمن الأرض ومن فيها إن كنتم تعلمون', 0),
(2780, 85, 'سَیَقُوْلُوْنَ لِلّٰهِؕ-قُلْ اَفَلَا تَذَكَّرُوْنَ(۸۵)', 0, 5, 5, 18, 4, 1, 23, 'سيقولون لله ۚ قل أفلا تذكرون', 0),
(2781, 86, 'قُلْ مَنْ رَّبُّ السَّمٰوٰتِ السَّبْعِ وَ رَبُّ الْعَرْشِ الْعَظِیْمِ(۸۶)', 0, 5, 5, 18, 4, 1, 23, 'قل من رب السماوات السبع ورب العرش العظيم', 0),
(2782, 87, 'سَیَقُوْلُوْنَ لِلّٰهِؕ-قُلْ اَفَلَا تَتَّقُوْنَ(۸۷)', 0, 5, 5, 18, 4, 1, 23, 'سيقولون لله ۚ قل أفلا تتقون', 0),
(2783, 88, 'قُلْ مَنْۢ بِیَدِهٖ مَلَكُوْتُ كُلِّ شَیْءٍ وَّ هُوَ یُجِیْرُ وَ لَا یُجَارُ عَلَیْهِ اِنْ كُنْتُمْ تَعْلَمُوْنَ(۸۸)', 0, 5, 5, 18, 4, 1, 23, 'قل من بيده ملكوت كل شيء وهو يجير ولا يجار عليه إن كنتم تعلمون', 0),
(2784, 89, 'سَیَقُوْلُوْنَ لِلّٰهِؕ-قُلْ فَاَنّٰى تُسْحَرُوْنَ(۸۹)', 0, 5, 5, 18, 4, 1, 23, 'سيقولون لله ۚ قل فأنى تسحرون', 0),
(2785, 90, 'بَلْ اَتَیْنٰهُمْ بِالْحَقِّ وَ اِنَّهُمْ لَكٰذِبُوْنَ(۹۰)', 0, 5, 5, 18, 4, 1, 23, 'بل أتيناهم بالحق وإنهم لكاذبون', 0),
(2786, 91, 'مَا اتَّخَذَ اللّٰهُ مِنْ وَّلَدٍ وَّ مَا كَانَ مَعَهٗ مِنْ اِلٰهٍ اِذًا لَّذَهَبَ كُلُّ اِلٰهٍۭ بِمَا خَلَقَ وَ لَعَلَا بَعْضُهُمْ عَلٰى بَعْضٍؕ-سُبْحٰنَ اللّٰهِ عَمَّا یَصِفُوْنَۙ(۹۱)', 0, 5, 5, 18, 4, 1, 23, 'ما اتخذ الله من ولد وما كان معه من إله ۚ إذا لذهب كل إله بما خلق ولعلا بعضهم على بعض ۚ سبحان الله عما يصفون', 0),
(2787, 92, 'عٰلِمِ الْغَیْبِ وَ الشَّهَادَةِ فَتَعٰلٰى عَمَّا یُشْرِكُوْنَ۠(۹۲)', 0, 5, 5, 18, 4, 1, 23, 'عالم الغيب والشهادة فتعالى عما يشركون', 0),
(2788, 93, 'قُلْ رَّبِّ اِمَّا تُرِیَنِّیْ مَا یُوْعَدُوْنَۙ(۹۳)', 0, 6, 6, 18, 4, 1, 23, 'قل رب إما تريني ما يوعدون', 0),
(2789, 94, 'رَبِّ فَلَا تَجْعَلْنِیْ فِی الْقَوْمِ الظّٰلِمِیْنَ(۹۴)', 0, 6, 6, 18, 4, 1, 23, 'رب فلا تجعلني في القوم الظالمين', 0),
(2790, 95, 'وَ اِنَّا عَلٰۤى اَنْ نُّرِیَكَ مَا نَعِدُهُمْ لَقٰدِرُوْنَ(۹۵)', 0, 6, 6, 18, 4, 1, 23, 'وإنا على أن نريك ما نعدهم لقادرون', 0),
(2791, 96, 'اِدْفَعْ بِالَّتِیْ هِیَ اَحْسَنُ السَّیِّئَةَؕ-نَحْنُ اَعْلَمُ بِمَا یَصِفُوْنَ(۹۶)', 0, 6, 6, 18, 4, 1, 23, 'ادفع بالتي هي أحسن السيئة ۚ نحن أعلم بما يصفون', 0),
(2792, 97, 'وَ قُلْ رَّبِّ اَعُوْذُ بِكَ مِنْ هَمَزٰتِ الشَّیٰطِیْنِۙ(۹۷)', 0, 6, 6, 18, 4, 1, 23, 'وقل رب أعوذ بك من همزات الشياطين', 0),
(2793, 98, 'وَ اَعُوْذُ بِكَ رَبِّ اَنْ یَّحْضُرُوْنِ(۹۸)', 0, 6, 6, 18, 4, 1, 23, 'وأعوذ بك رب أن يحضرون', 0),
(2794, 99, 'حَتّٰۤى اِذَا جَآءَ اَحَدَهُمُ الْمَوْتُ قَالَ رَبِّ ارْجِعُوْنِۙ(۹۹)', 0, 6, 6, 18, 4, 1, 23, 'حتى إذا جاء أحدهم الموت قال رب ارجعون', 0),
(2795, 100, 'لَعَلِّیْۤ اَعْمَلُ صَالِحًا فِیْمَا تَرَكْتُ كَلَّاؕ-اِنَّهَا كَلِمَةٌ هُوَ قَآىٕلُهَاؕ-وَ مِنْ وَّرَآىٕهِمْ بَرْزَخٌ اِلٰى یَوْمِ یُبْعَثُوْنَ(۱۰۰)', 0, 6, 6, 18, 4, 1, 23, 'لعلي أعمل صالحا فيما تركت ۚ كلا ۚ إنها كلمة هو قائلها ۖ ومن ورائهم برزخ إلى يوم يبعثون', 0),
(2796, 101, 'فَاِذَا نُفِخَ فِی الصُّوْرِ فَلَاۤ اَنْسَابَ بَیْنَهُمْ یَوْمَىٕذٍ وَّ لَا یَتَسَآءَلُوْنَ(۱۰۱)', 0, 6, 6, 18, 4, 1, 23, 'فإذا نفخ في الصور فلا أنساب بينهم يومئذ ولا يتساءلون', 0),
(2797, 102, 'فَمَنْ ثَقُلَتْ مَوَازِیْنُهٗ فَاُولٰٓىٕكَ هُمُ الْمُفْلِحُوْنَ(۱۰۲)', 0, 6, 6, 18, 4, 1, 23, 'فمن ثقلت موازينه فأولئك هم المفلحون', 0),
(2798, 103, 'وَ مَنْ خَفَّتْ مَوَازِیْنُهٗ فَاُولٰٓىٕكَ الَّذِیْنَ خَسِرُوْۤا اَنْفُسَهُمْ فِیْ جَهَنَّمَ خٰلِدُوْنَۚ(۱۰۳)', 0, 6, 6, 18, 4, 1, 23, 'ومن خفت موازينه فأولئك الذين خسروا أنفسهم في جهنم خالدون', 0),
(2799, 104, 'تَلْفَحُ وُجُوْهَهُمُ النَّارُ وَ هُمْ فِیْهَا كٰلِحُوْنَ(۱۰۴)', 0, 6, 6, 18, 4, 1, 23, 'تلفح وجوههم النار وهم فيها كالحون', 0),
(2800, 105, 'اَلَمْ تَكُنْ اٰیٰتِیْ تُتْلٰى عَلَیْكُمْ فَكُنْتُمْ بِهَا تُكَذِّبُوْنَ(۱۰۵)', 0, 6, 6, 18, 4, 1, 23, 'ألم تكن آياتي تتلى عليكم فكنتم بها تكذبون', 0),
(2801, 106, 'قَالُوْا رَبَّنَا غَلَبَتْ عَلَیْنَا شِقْوَتُنَا وَ كُنَّا قَوْمًا ضَآلِّیْنَ(۱۰۶)', 0, 6, 6, 18, 4, 1, 23, 'قالوا ربنا غلبت علينا شقوتنا وكنا قوما ضالين', 0),
(2802, 107, 'رَبَّنَاۤ اَخْرِجْنَا مِنْهَا فَاِنْ عُدْنَا فَاِنَّا ظٰلِمُوْنَ(۱۰۷)', 0, 6, 6, 18, 4, 1, 23, 'ربنا أخرجنا منها فإن عدنا فإنا ظالمون', 0);
INSERT INTO `quran` (`ayatId`, `ayatNumber`, `arabicText`, `ayatSajda`, `surahRuku`, `paraRuku`, `paraId`, `manzilNo`, `ayatVisible`, `surahId`, `withoutAerab`, `favourite`) VALUES
(2803, 108, 'قَالَ اخْسَــٴُـوْا فِیْهَا وَ لَا تُكَلِّمُوْنِ(۱۰۸)', 0, 6, 6, 18, 4, 1, 23, 'قال اخسئوا فيها ولا تكلمون', 0),
(2804, 109, 'اِنَّهٗ كَانَ فَرِیْقٌ مِّنْ عِبَادِیْ یَقُوْلُوْنَ رَبَّنَاۤ اٰمَنَّا فَاغْفِرْ لَنَا وَ ارْحَمْنَا وَ اَنْتَ خَیْرُ الرّٰحِمِیْنَۚۖ(۱۰۹)', 0, 6, 6, 18, 4, 1, 23, 'إنه كان فريق من عبادي يقولون ربنا آمنا فاغفر لنا وارحمنا وأنت خير الراحمين', 0),
(2805, 110, 'فَاتَّخَذْتُمُوْهُمْ سِخْرِیًّا حَتّٰۤى اَنْسَوْكُمْ ذِكْرِیْ وَ كُنْتُمْ مِّنْهُمْ تَضْحَكُوْنَ(۱۱۰)', 0, 6, 6, 18, 4, 1, 23, 'فاتخذتموهم سخريا حتى أنسوكم ذكري وكنتم منهم تضحكون', 0),
(2806, 111, 'اِنِّیْ جَزَیْتُهُمُ الْیَوْمَ بِمَا صَبَرُوْۤاۙ-اَنَّهُمْ هُمُ الْفَآىٕزُوْنَ(۱۱۱)', 0, 6, 6, 18, 4, 1, 23, 'إني جزيتهم اليوم بما صبروا أنهم هم الفائزون', 0),
(2807, 112, 'قٰلَ كَمْ لَبِثْتُمْ فِی الْاَرْضِ عَدَدَ سِنِیْنَ(۱۱۲)', 0, 6, 6, 18, 4, 1, 23, 'قال كم لبثتم في الأرض عدد سنين', 0),
(2808, 113, 'قَالُوْا لَبِثْنَا یَوْمًا اَوْ بَعْضَ یَوْمٍ فَسْــٴَـلِ الْعَآدِّیْنَ(۱۱۳)', 0, 6, 6, 18, 4, 1, 23, 'قالوا لبثنا يوما أو بعض يوم فاسأل العادين', 0),
(2809, 114, 'قٰلَ اِنْ لَّبِثْتُمْ اِلَّا قَلِیْلًا لَّوْ اَنَّكُمْ كُنْتُمْ تَعْلَمُوْنَ(۱۱۴)', 0, 6, 6, 18, 4, 1, 23, 'قال إن لبثتم إلا قليلا ۖ لو أنكم كنتم تعلمون', 0),
(2810, 115, 'اَفَحَسِبْتُمْ اَنَّمَا خَلَقْنٰكُمْ عَبَثًا وَّ اَنَّكُمْ اِلَیْنَا لَا تُرْجَعُوْنَ(۱۱۵)', 0, 6, 6, 18, 4, 1, 23, 'أفحسبتم أنما خلقناكم عبثا وأنكم إلينا لا ترجعون', 0),
(2811, 116, 'فَتَعٰلَى اللّٰهُ الْمَلِكُ الْحَقُّۚ-لَاۤ اِلٰهَ اِلَّا هُوَۚ-رَبُّ الْعَرْشِ الْكَرِیْمِ(۱۱۶)', 0, 6, 6, 18, 4, 1, 23, 'فتعالى الله الملك الحق ۖ لا إله إلا هو رب العرش الكريم', 0),
(2812, 117, 'وَ مَنْ یَّدْعُ مَعَ اللّٰهِ اِلٰهًا اٰخَرَۙ-لَا بُرْهَانَ لَهٗ بِهٖۙ-فَاِنَّمَا حِسَابُهٗ عِنْدَ رَبِّهٖؕ-اِنَّهٗ لَا یُفْلِحُ الْكٰفِرُوْنَ(۱۱۷)', 0, 6, 6, 18, 4, 1, 23, 'ومن يدع مع الله إلها آخر لا برهان له به فإنما حسابه عند ربه ۚ إنه لا يفلح الكافرون', 0),
(2813, 118, 'وَ قُلْ رَّبِّ اغْفِرْ وَ ارْحَمْ وَ اَنْتَ خَیْرُ الرّٰحِمِیْنَ۠(۱۱۸)', 0, 6, 6, 18, 4, 1, 23, 'وقل رب اغفر وارحم وأنت خير الراحمين', 0),
(2814, 0, 'بِسْمِ اللّٰهِ الرَّحْمٰنِ الرَّحِیْمِ', 0, 1, 7, 18, 7, 1, 24, 'بسم الله الرحمن الرحيم', 0),
(2815, 1, 'سُوْرَةٌ اَنْزَلْنٰهَا وَ فَرَضْنٰهَا وَ اَنْزَلْنَا فِیْهَاۤ اٰیٰتٍۭ بَیِّنٰتٍ لَّعَلَّكُمْ تَذَكَّرُوْنَ(۱)', 0, 1, 7, 18, 4, 1, 24, 'سورة أنزلناها وفرضناها وأنزلنا فيها آيات بينات لعلكم تذكرون', 0),
(2816, 2, 'اَلزَّانِیَةُ وَ الزَّانِیْ فَاجْلِدُوْا كُلَّ وَاحِدٍ مِّنْهُمَا مِائَةَ جَلْدَةٍ۪-وَّ لَا تَاْخُذْكُمْ بِهِمَا رَاْفَةٌ فِیْ دِیْنِ اللّٰهِ اِنْ كُنْتُمْ تُؤْمِنُوْنَ بِاللّٰهِ وَ الْیَوْمِ الْاٰخِرِۚ-وَ لْیَشْهَدْ عَذَابَهُمَا طَآىٕفَةٌ مِّنَ الْمُؤْمِنِیْنَ(۲)', 0, 1, 7, 18, 4, 1, 24, 'الزانية والزاني فاجلدوا كل واحد منهما مائة جلدة ۖ ولا تأخذكم بهما رأفة في دين الله إن كنتم تؤمنون بالله واليوم الآخر ۖ وليشهد عذابهما طائفة من المؤمنين', 0),
(2817, 3, 'اَلزَّانِیْ لَا یَنْكِحُ اِلَّا زَانِیَةً اَوْ مُشْرِكَةً٘-وَّ الزَّانِیَةُ لَا یَنْكِحُهَاۤ اِلَّا زَانٍ اَوْ مُشْرِكٌۚ-وَ حُرِّمَ ذٰلِكَ عَلَى الْمُؤْمِنِیْنَ(۳)', 0, 1, 7, 18, 4, 1, 24, 'الزاني لا ينكح إلا زانية أو مشركة والزانية لا ينكحها إلا زان أو مشرك ۚ وحرم ذلك على المؤمنين', 0),
