(2292, 24, 'فَنَادٰىهَا مِنْ تَحْتِهَاۤ اَلَّا تَحْزَنِیْ قَدْ جَعَلَ رَبُّكِ تَحْتَكِ سَرِیًّا(۲۴)', 0, 2, 5, 16, 4, 1, 19, 'فناداها من تحتها ألا تحزني قد جعل ربك تحتك سريا', 0),
(2293, 25, 'وَ هُزِّیْۤ اِلَیْكِ بِجِذْعِ النَّخْلَةِ تُسٰقِطْ عَلَیْكِ رُطَبًا جَنِیًّا٘(۲۵)', 0, 2, 5, 16, 4, 1, 19, 'وهزي إليك بجذع النخلة تساقط عليك رطبا جنيا', 0),
(2294, 26, 'فَكُلِیْ وَ اشْرَبِیْ وَ قَرِّیْ عَیْنًاۚ-فَاِمَّا تَرَیِنَّ مِنَ الْبَشَرِ اَحَدًاۙ-فَقُوْلِیْۤ اِنِّیْ نَذَرْتُ لِلرَّحْمٰنِ صَوْمًا فَلَنْ اُكَلِّمَ الْیَوْمَ اِنْسِیًّاۚ(۲۶)', 0, 2, 5, 16, 4, 1, 19, 'فكلي واشربي وقري عينا ۖ فإما ترين من البشر أحدا فقولي إني نذرت للرحمن صوما فلن أكلم اليوم إنسيا', 0),
(2295, 27, 'فَاَتَتْ بِهٖ قَوْمَهَا تَحْمِلُهٗؕ-قَالُوْا یٰمَرْیَمُ لَقَدْ جِئْتِ شَیْــٴًـا فَرِیًّا(۲۷)', 0, 2, 5, 16, 4, 1, 19, 'فأتت به قومها تحمله ۖ قالوا يا مريم لقد جئت شيئا فريا', 0),
(2296, 28, 'یٰۤاُخْتَ هٰرُوْنَ مَا كَانَ اَبُوْكِ امْرَاَ سَوْءٍ وَّ مَا كَانَتْ اُمُّكِ بَغِیًّاۖۚ(۲۸)', 0, 2, 5, 16, 4, 1, 19, 'يا أخت هارون ما كان أبوك امرأ سوء وما كانت أمك بغيا', 0),
(2297, 29, 'فَاَشَارَتْ اِلَیْهِؕ-قَالُوْا كَیْفَ نُكَلِّمُ مَنْ كَانَ فِی الْمَهْدِ صَبِیًّا(۲۹)', 0, 2, 5, 16, 4, 1, 19, 'فأشارت إليه ۖ قالوا كيف نكلم من كان في المهد صبيا', 0),
(2298, 30, 'قَالَ اِنِّیْ عَبْدُ اللّٰهِ ﳴ اٰتٰىنِیَ الْكِتٰبَ وَ جَعَلَنِیْ نَبِیًّاۙ(۳۰)', 0, 2, 5, 16, 4, 1, 19, 'قال إني عبد الله آتاني الكتاب وجعلني نبيا', 0),
(2299, 31, 'وَّ جَعَلَنِیْ مُبٰرَكًا اَیْنَ مَا كُنْتُ۪-وَ اَوْصٰنِیْ بِالصَّلٰوةِ وَ الزَّكٰوةِ مَا دُمْتُ حَیًّاﳚ(۳۱)', 0, 2, 5, 16, 4, 1, 19, 'وجعلني مباركا أين ما كنت وأوصاني بالصلاة والزكاة ما دمت حيا', 0),
(2300, 32, 'وَّ بَرًّۢا بِوَالِدَتِیْ٘-وَ لَمْ یَجْعَلْنِیْ جَبَّارًا شَقِیًّا(۳۲)', 0, 2, 5, 16, 4, 1, 19, 'وبرا بوالدتي ولم يجعلني جبارا شقيا', 0),
(2301, 33, 'وَ السَّلٰمُ عَلَیَّ یَوْمَ وُلِدْتُّ وَ یَوْمَ اَمُوْتُ وَ یَوْمَ اُبْعَثُ حَیًّا(۳۳)', 0, 2, 5, 16, 4, 1, 19, 'والسلام علي يوم ولدت ويوم أموت ويوم أبعث حيا', 0),
(2302, 34, 'ذٰلِكَ عِیْسَى ابْنُ مَرْیَمَۚ-قَوْلَ الْحَقِّ الَّذِیْ فِیْهِ یَمْتَرُوْنَ(۳۴)', 0, 2, 5, 16, 4, 1, 19, 'ذلك عيسى ابن مريم ۚ قول الحق الذي فيه يمترون', 0),
(2303, 35, 'مَا كَانَ لِلّٰهِ اَنْ یَّتَّخِذَ مِنْ وَّلَدٍۙ-سُبْحٰنَهٗؕ-اِذَا قَضٰۤى اَمْرًا فَاِنَّمَا یَقُوْلُ لَهٗ كُنْ فَیَكُوْنُؕ(۳۵)', 0, 2, 5, 16, 4, 1, 19, 'ما كان لله أن يتخذ من ولد ۖ سبحانه ۚ إذا قضى أمرا فإنما يقول له كن فيكون', 0),
(2304, 36, 'وَ اِنَّ اللّٰهَ رَبِّیْ وَ رَبُّكُمْ فَاعْبُدُوْهُؕ-هٰذَا صِرَاطٌ مُّسْتَقِیْمٌ(۳۶)', 0, 2, 5, 16, 4, 1, 19, 'وإن الله ربي وربكم فاعبدوه ۚ هذا صراط مستقيم', 0),
(2305, 37, 'فَاخْتَلَفَ الْاَحْزَابُ مِنْۢ بَیْنِهِمْۚ-فَوَیْلٌ لِّلَّذِیْنَ كَفَرُوْا مِنْ مَّشْهَدِ یَوْمٍ عَظِیْمٍ(۳۷)', 0, 2, 5, 16, 4, 1, 19, 'فاختلف الأحزاب من بينهم ۖ فويل للذين كفروا من مشهد يوم عظيم', 0),
(2306, 38, 'اَسْمِعْ بِهِمْ وَ اَبْصِرْۙ-یَوْمَ یَاْتُوْنَنَا لٰكِنِ الظّٰلِمُوْنَ الْیَوْمَ فِیْ ضَلٰلٍ مُّبِیْنٍ(۳۸)', 0, 2, 5, 16, 4, 1, 19, 'أسمع بهم وأبصر يوم يأتوننا ۖ لكن الظالمون اليوم في ضلال مبين', 0),
(2307, 39, 'وَ اَنْذِرْهُمْ یَوْمَ الْحَسْرَةِ اِذْ قُضِیَ الْاَمْرُۘ-وَ هُمْ فِیْ غَفْلَةٍ وَّ هُمْ لَا یُؤْمِنُوْنَ(۳۹)', 0, 2, 5, 16, 4, 1, 19, 'وأنذرهم يوم الحسرة إذ قضي الأمر وهم في غفلة وهم لا يؤمنون', 0),
(2308, 40, 'اِنَّا نَحْنُ نَرِثُ الْاَرْضَ وَ مَنْ عَلَیْهَا وَ اِلَیْنَا یُرْجَعُوْنَ۠(۴۰)', 0, 2, 5, 16, 4, 1, 19, 'إنا نحن نرث الأرض ومن عليها وإلينا يرجعون', 0),
(2309, 41, 'وَ اذْكُرْ فِی الْكِتٰبِ اِبْرٰهِیْمَ۬ؕ-اِنَّهٗ كَانَ صِدِّیْقًا نَّبِیًّا(۴۱)', 0, 3, 6, 16, 4, 1, 19, 'واذكر في الكتاب إبراهيم ۚ إنه كان صديقا نبيا', 0),
(2310, 42, 'اِذْ قَالَ لِاَبِیْهِ یٰۤاَبَتِ لِمَ تَعْبُدُ مَا لَا یَسْمَعُ وَ لَا یُبْصِرُ وَ لَا یُغْنِیْ عَنْكَ شَیْــٴًـا(۴۲)', 0, 3, 6, 16, 4, 1, 19, 'إذ قال لأبيه يا أبت لم تعبد ما لا يسمع ولا يبصر ولا يغني عنك شيئا', 0),
(2311, 43, 'یٰۤاَبَتِ اِنِّیْ قَدْ جَآءَنِیْ مِنَ الْعِلْمِ مَا لَمْ یَاْتِكَ فَاتَّبِعْنِیْۤ اَهْدِكَ صِرَاطًا سَوِیًّا(۴۳)', 0, 3, 6, 16, 4, 1, 19, 'يا أبت إني قد جاءني من العلم ما لم يأتك فاتبعني أهدك صراطا سويا', 0),
(2312, 44, 'یٰۤاَبَتِ لَا تَعْبُدِ الشَّیْطٰنَؕ-اِنَّ الشَّیْطٰنَ كَانَ لِلرَّحْمٰنِ عَصِیًّا(۴۴)', 0, 3, 6, 16, 4, 1, 19, 'يا أبت لا تعبد الشيطان ۖ إن الشيطان كان للرحمن عصيا', 0);
INSERT INTO `quran` (`ayatId`, `ayatNumber`, `arabicText`, `ayatSajda`, `surahRuku`, `paraRuku`, `paraId`, `manzilNo`, `ayatVisible`, `surahId`, `withoutAerab`, `favourite`) VALUES
(2313, 45, 'یٰۤاَبَتِ اِنِّیْۤ اَخَافُ اَنْ یَّمَسَّكَ عَذَابٌ مِّنَ الرَّحْمٰنِ فَتَكُوْنَ لِلشَّیْطٰنِ وَلِیًّا(۴۵)', 0, 3, 6, 16, 4, 1, 19, 'يا أبت إني أخاف أن يمسك عذاب من الرحمن فتكون للشيطان وليا', 0),
(2314, 46, 'قَالَ اَرَاغِبٌ اَنْتَ عَنْ اٰلِهَتِیْ یٰۤاِبْرٰهِیْمُۚ-لَىٕنْ لَّمْ تَنْتَهِ لَاَرْجُمَنَّكَ وَ اهْجُرْنِیْ مَلِیًّا(۴۶)', 0, 3, 6, 16, 4, 1, 19, 'قال أراغب أنت عن آلهتي يا إبراهيم ۖ لئن لم تنته لأرجمنك ۖ واهجرني مليا', 0),
(2315, 47, 'قَالَ سَلٰمٌ عَلَیْكَۚ-سَاَسْتَغْفِرُ لَكَ رَبِّیْؕ-اِنَّهٗ كَانَ بِیْ حَفِیًّا(۴۷)', 0, 3, 6, 16, 4, 1, 19, 'قال سلام عليك ۖ سأستغفر لك ربي ۖ إنه كان بي حفيا', 0),
(2316, 48, 'وَ اَعْتَزِلُكُمْ وَ مَا تَدْعُوْنَ مِنْ دُوْنِ اللّٰهِ وَ اَدْعُوْا رَبِّیْ ﳲ عَسٰۤى اَلَّاۤ اَكُوْنَ بِدُعَآءِ رَبِّیْ شَقِیًّا(۴۸)', 0, 3, 6, 16, 4, 1, 19, 'وأعتزلكم وما تدعون من دون الله وأدعو ربي عسى ألا أكون بدعاء ربي شقيا', 0),
(2317, 49, 'فَلَمَّا اعْتَزَلَهُمْ وَ مَا یَعْبُدُوْنَ مِنْ دُوْنِ اللّٰهِۙ-وَهَبْنَا لَهٗۤ اِسْحٰقَ وَ یَعْقُوْبَؕ-وَ كُلًّا جَعَلْنَا نَبِیًّا(۴۹)', 0, 3, 6, 16, 4, 1, 19, 'فلما اعتزلهم وما يعبدون من دون الله وهبنا له إسحاق ويعقوب ۖ وكلا جعلنا نبيا', 0),
(2318, 50, 'وَ وَهَبْنَا لَهُمْ مِّنْ رَّحْمَتِنَا وَ جَعَلْنَا لَهُمْ لِسَانَ صِدْقٍ عَلِیًّا۠(۵۰)', 0, 3, 6, 16, 4, 1, 19, 'ووهبنا لهم من رحمتنا وجعلنا لهم لسان صدق عليا', 0),
(2319, 51, 'وَ اذْكُرْ فِی الْكِتٰبِ مُوْسٰۤى٘-اِنَّهٗ كَانَ مُخْلَصًا وَّ كَانَ رَسُوْلًا نَّبِیًّا(۵۱)', 0, 4, 7, 16, 4, 1, 19, 'واذكر في الكتاب موسى ۚ إنه كان مخلصا وكان رسولا نبيا', 0),
(2320, 52, 'وَ نَادَیْنٰهُ مِنْ جَانِبِ الطُّوْرِ الْاَیْمَنِ وَ قَرَّبْنٰهُ نَجِیًّا(۵۲)', 0, 4, 7, 16, 4, 1, 19, 'وناديناه من جانب الطور الأيمن وقربناه نجيا', 0),
(2321, 53, 'وَ وَهَبْنَا لَهٗ مِنْ رَّحْمَتِنَاۤ اَخَاهُ هٰرُوْنَ نَبِیًّا(۵۳)', 0, 4, 7, 16, 4, 1, 19, 'ووهبنا له من رحمتنا أخاه هارون نبيا', 0),
(2322, 54, 'وَ اذْكُرْ فِی الْكِتٰبِ اِسْمٰعِیْلَ٘-اِنَّهٗ كَانَ صَادِقَ الْوَعْدِ وَ كَانَ رَسُوْلًا نَّبِیًّاۚ(۵۴)', 0, 4, 7, 16, 4, 1, 19, 'واذكر في الكتاب إسماعيل ۚ إنه كان صادق الوعد وكان رسولا نبيا', 0),
(2323, 55, 'وَ كَانَ یَاْمُرُ اَهْلَهٗ بِالصَّلٰوةِ وَ الزَّكٰوةِ۪-وَ كَانَ عِنْدَ رَبِّهٖ مَرْضِیًّا(۵۵)', 0, 4, 7, 16, 4, 1, 19, 'وكان يأمر أهله بالصلاة والزكاة وكان عند ربه مرضيا', 0),
(2324, 56, 'وَ اذْكُرْ فِی الْكِتٰبِ اِدْرِیْسَ٘-اِنَّهٗ كَانَ صِدِّیْقًا نَّبِیًّاۗۙ(۵۶)', 0, 4, 7, 16, 4, 1, 19, 'واذكر في الكتاب إدريس ۚ إنه كان صديقا نبيا', 0),
(2325, 57, 'وَّ رَفَعْنٰهُ مَكَانًا عَلِیًّا(۵۷)', 0, 4, 7, 16, 4, 1, 19, 'ورفعناه مكانا عليا', 0),
(2326, 58, 'اُولٰٓىٕكَ الَّذِیْنَ اَنْعَمَ اللّٰهُ عَلَیْهِمْ مِّنَ النَّبِیّٖنَ مِنْ ذُرِّیَّةِ اٰدَمَۗ-وَ مِمَّنْ حَمَلْنَا مَعَ نُوْحٍ٘-وَّ مِنْ ذُرِّیَّةِ اِبْرٰهِیْمَ وَ اِسْرَآءِیْلَ٘-وَ مِمَّنْ هَدَیْنَا وَ اجْتَبَیْنَاؕ-اِذَا تُتْلٰى عَلَیْهِمْ اٰیٰتُ الرَّحْمٰنِ خَرُّوْا سُجَّدًا وَّ بُكِیًّا۩(۵۸)', 1, 4, 7, 16, 4, 1, 19, 'أولئك الذين أنعم الله عليهم من النبيين من ذرية آدم وممن حملنا مع نوح ومن ذرية إبراهيم وإسرائيل وممن هدينا واجتبينا ۚ إذا تتلى عليهم آيات الرحمن خروا سجدا وبكيا', 0),
(2327, 59, 'فَخَلَفَ مِنْۢ بَعْدِهِمْ خَلْفٌ اَضَاعُوا الصَّلٰوةَ وَ اتَّبَعُوا الشَّهَوٰتِ فَسَوْفَ یَلْقَوْنَ غَیًّاۙ(۵۹)', 0, 4, 7, 16, 4, 1, 19, 'فخلف من بعدهم خلف أضاعوا الصلاة واتبعوا الشهوات ۖ فسوف يلقون غيا', 0),
(2328, 60, 'اِلَّا مَنْ تَابَ وَ اٰمَنَ وَ عَمِلَ صَالِحًا فَاُولٰٓىٕكَ یَدْخُلُوْنَ الْجَنَّةَ وَ لَا یُظْلَمُوْنَ شَیْــٴًـاۙ(۶۰)', 0, 4, 7, 16, 4, 1, 19, 'إلا من تاب وآمن وعمل صالحا فأولئك يدخلون الجنة ولا يظلمون شيئا', 0),
(2329, 61, 'جَنّٰتِ عَدْنِ ﹰالَّتِیْ وَعَدَ الرَّحْمٰنُ عِبَادَهٗ بِالْغَیْبِؕ-اِنَّهٗ كَانَ وَعْدُهٗ مَاْتِیًّا(۶۱)', 0, 4, 7, 16, 4, 1, 19, 'جنات عدن التي وعد الرحمن عباده بالغيب ۚ إنه كان وعده مأتيا', 0),
(2330, 62, 'لَا یَسْمَعُوْنَ فِیْهَا لَغْوًا اِلَّا سَلٰمًاؕ-وَ لَهُمْ رِزْقُهُمْ فِیْهَا بُكْرَةً وَّ عَشِیًّا(۶۲)', 0, 4, 7, 16, 4, 1, 19, 'لا يسمعون فيها لغوا إلا سلاما ۖ ولهم رزقهم فيها بكرة وعشيا', 0),
(2331, 63, 'تِلْكَ الْجَنَّةُ الَّتِیْ نُوْرِثُ مِنْ عِبَادِنَا مَنْ كَانَ تَقِیًّا(۶۳)', 0, 4, 7, 16, 4, 1, 19, 'تلك الجنة التي نورث من عبادنا من كان تقيا', 0),
(2332, 64, 'وَ مَا نَتَنَزَّلُ اِلَّا بِاَمْرِ رَبِّكَۚ-لَهٗ مَا بَیْنَ اَیْدِیْنَا وَ مَا خَلْفَنَا وَ مَا بَیْنَ ذٰلِكَۚ-وَ مَا كَانَ رَبُّكَ نَسِیًّاۚ(۶۴)', 0, 4, 7, 16, 4, 1, 19, 'وما نتنزل إلا بأمر ربك ۖ له ما بين أيدينا وما خلفنا وما بين ذلك ۚ وما كان ربك نسيا', 0),
(2333, 65, 'رَبُّ السَّمٰوٰتِ وَ الْاَرْضِ وَ مَا بَیْنَهُمَا فَاعْبُدْهُ وَ اصْطَبِرْ لِعِبَادَتِهٖؕ-هَلْ تَعْلَمُ لَهٗ سَمِیًّا۠(۶۵)', 0, 4, 7, 16, 4, 1, 19, 'رب السماوات والأرض وما بينهما فاعبده واصطبر لعبادته ۚ هل تعلم له سميا', 0),
