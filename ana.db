(1902, 86, 'اِنَّ رَبَّكَ هُوَ الْخَلّٰقُ الْعَلِیْمُ(۸۶)', 0, 6, 6, 14, 3, 1, 15, 'إن ربك هو الخلاق العليم', 0),
(1903, 87, 'وَ لَقَدْ اٰتَیْنٰكَ سَبْعًا مِّنَ الْمَثَانِیْ وَ الْقُرْاٰنَ الْعَظِیْمَ(۸۷)', 0, 6, 6, 14, 3, 1, 15, 'ولقد آتيناك سبعا من المثاني والقرآن العظيم', 0),
(1904, 88, 'لَا تَمُدَّنَّ عَیْنَیْكَ اِلٰى مَا مَتَّعْنَا بِهٖۤ اَزْوَاجًا مِّنْهُمْ وَ لَا تَحْزَنْ عَلَیْهِمْ وَ اخْفِضْ جَنَاحَكَ لِلْمُؤْمِنِیْنَ(۸۸)', 0, 6, 6, 14, 3, 1, 15, 'لا تمدن عينيك إلى ما متعنا به أزواجا منهم ولا تحزن عليهم واخفض جناحك للمؤمنين', 0),
(1905, 89, 'وَ قُلْ اِنِّیْۤ اَنَا النَّذِیْرُ الْمُبِیْنُۚ(۸۹)', 0, 6, 6, 14, 3, 1, 15, 'وقل إني أنا النذير المبين', 0),
(1906, 90, 'كَمَاۤ اَنْزَلْنَا عَلَى الْمُقْتَسِمِیْنَۙ(۹۰)', 0, 6, 6, 14, 3, 1, 15, 'كما أنزلنا على المقتسمين', 0),
(1907, 91, 'الَّذِیْنَ جَعَلُوا الْقُرْاٰنَ عِضِیْنَ(۹۱)', 0, 6, 6, 14, 3, 1, 15, 'الذين جعلوا القرآن عضين', 0),
(1908, 92, 'فَوَرَبِّكَ لَنَسْــٴَـلَنَّهُمْ اَجْمَعِیْنَۙ(۹۲)', 0, 6, 6, 14, 3, 1, 15, 'فوربك لنسألنهم أجمعين', 0),
(1909, 93, 'عَمَّا كَانُوْا یَعْمَلُوْنَ(۹۳)', 0, 6, 6, 14, 3, 1, 15, 'عما كانوا يعملون', 0),
(1910, 94, 'فَاصْدَعْ بِمَا تُؤْمَرُ وَ اَعْرِضْ عَنِ الْمُشْرِكِیْنَ(۹۴)', 0, 6, 6, 14, 3, 1, 15, 'فاصدع بما تؤمر وأعرض عن المشركين', 0),
(1911, 95, 'اِنَّا كَفَیْنٰكَ الْمُسْتَهْزِءِیْنَۙ(۹۵)', 0, 6, 6, 14, 3, 1, 15, 'إنا كفيناك المستهزئين', 0),
(1912, 96, 'الَّذِیْنَ یَجْعَلُوْنَ مَعَ اللّٰهِ اِلٰهًا اٰخَرَۚ-فَسَوْفَ یَعْلَمُوْنَ(۹۶)', 0, 6, 6, 14, 3, 1, 15, 'الذين يجعلون مع الله إلها آخر ۚ فسوف يعلمون', 0),
(1913, 97, 'وَ لَقَدْ نَعْلَمُ اَنَّكَ یَضِیْقُ صَدْرُكَ بِمَا یَقُوْلُوْنَۙ(۹۷)', 0, 6, 6, 14, 3, 1, 15, 'ولقد نعلم أنك يضيق صدرك بما يقولون', 0),
(1914, 98, 'فَسَبِّحْ بِحَمْدِ رَبِّكَ وَ كُنْ مِّنَ السّٰجِدِیْنَۙ(۹۸)', 0, 6, 6, 14, 3, 1, 15, 'فسبح بحمد ربك وكن من الساجدين', 0),
(1915, 99, 'وَ اعْبُدْ رَبَّكَ حَتّٰى یَاْتِیَكَ الْیَقِیْنُ۠(۹۹)', 0, 6, 6, 14, 3, 1, 15, 'واعبد ربك حتى يأتيك اليقين', 0),
(1916, 0, 'بِسْمِ اللّٰهِ الرَّحْمٰنِ الرَّحِیْمِ', 0, 1, 7, 14, 3, 1, 16, 'بسم الله الرحمن الرحيم', 0),
(1917, 1, 'اَتٰۤى اَمْرُ اللّٰهِ فَلَا تَسْتَعْجِلُوْهُؕ-سُبْحٰنَهٗ وَ تَعٰلٰى عَمَّا یُشْرِكُوْنَ(۱)', 0, 1, 7, 14, 3, 1, 16, 'أتى أمر الله فلا تستعجلوه ۚ سبحانه وتعالى عما يشركون', 0),
(1918, 2, 'یُنَزِّلُ الْمَلٰٓىٕكَةَ بِالرُّوْحِ مِنْ اَمْرِهٖ عَلٰى مَنْ یَّشَآءُ مِنْ عِبَادِهٖۤ اَنْ اَنْذِرُوْۤا اَنَّهٗ لَاۤ اِلٰهَ اِلَّاۤ اَنَا فَاتَّقُوْنِ(۲)', 0, 1, 7, 14, 3, 1, 16, 'ينزل الملائكة بالروح من أمره على من يشاء من عباده أن أنذروا أنه لا إله إلا أنا فاتقون', 0),
(1919, 3, 'خَلَقَ السَّمٰوٰتِ وَ الْاَرْضَ بِالْحَقِّؕ-تَعٰلٰى عَمَّا یُشْرِكُوْنَ(۳)', 0, 1, 7, 14, 3, 1, 16, 'خلق السماوات والأرض بالحق ۚ تعالى عما يشركون', 0),
(1920, 4, 'خَلَقَ الْاِنْسَانَ مِنْ نُّطْفَةٍ فَاِذَا هُوَ خَصِیْمٌ مُّبِیْنٌ(۴)', 0, 1, 7, 14, 3, 1, 16, 'خلق الإنسان من نطفة فإذا هو خصيم مبين', 0),
(1921, 5, 'وَ الْاَنْعَامَ خَلَقَهَاۚ-لَكُمْ فِیْهَا دِفْءٌ وَّ مَنَافِعُ وَ مِنْهَا تَاْكُلُوْنَ(۵)', 0, 1, 7, 14, 3, 1, 16, 'والأنعام خلقها ۗ لكم فيها دفء ومنافع ومنها تأكلون', 0),
(1922, 6, 'وَ لَكُمْ فِیْهَا جَمَالٌ حِیْنَ تُرِیْحُوْنَ وَ حِیْنَ تَسْرَحُوْنَ۪(۶)', 0, 1, 7, 14, 3, 1, 16, 'ولكم فيها جمال حين تريحون وحين تسرحون', 0),
(1923, 7, 'وَ تَحْمِلُ اَثْقَالَكُمْ اِلٰى بَلَدٍ لَّمْ تَكُوْنُوْا بٰلِغِیْهِ اِلَّا بِشِقِّ الْاَنْفُسِؕ-اِنَّ رَبَّكُمْ لَرَءُوْفٌ رَّحِیْمٌۙ(۷)', 0, 1, 7, 14, 3, 1, 16, 'وتحمل أثقالكم إلى بلد لم تكونوا بالغيه إلا بشق الأنفس ۚ إن ربكم لرءوف رحيم', 0),
(1924, 8, 'وَّ الْخَیْلَ وَ الْبِغَالَ وَ الْحَمِیْرَ لِتَرْكَبُوْهَا وَ زِیْنَةًؕ-وَ یَخْلُقُ مَا لَا تَعْلَمُوْنَ(۸)', 0, 1, 7, 14, 3, 1, 16, 'والخيل والبغال والحمير لتركبوها وزينة ۚ ويخلق ما لا تعلمون', 0),
(1925, 9, 'وَ عَلَى اللّٰهِ قَصْدُ السَّبِیْلِ وَ مِنْهَا جَآىٕرٌؕ-وَ لَوْ شَآءَ لَهَدٰىكُمْ اَجْمَعِیْنَ۠(۹)', 0, 1, 7, 14, 3, 1, 16, 'وعلى الله قصد السبيل ومنها جائر ۚ ولو شاء لهداكم أجمعين', 0),
(1926, 10, 'هُوَ الَّذِیْۤ اَنْزَلَ مِنَ السَّمَآءِ مَآءً لَّكُمْ مِّنْهُ شَرَابٌ وَّ مِنْهُ شَجَرٌ فِیْهِ تُسِیْمُوْنَ(۱۰)', 0, 2, 8, 14, 3, 1, 16, 'هو الذي أنزل من السماء ماء ۖ لكم منه شراب ومنه شجر فيه تسيمون', 0),
(1927, 11, 'یُنْۢبِتُ لَكُمْ بِهِ الزَّرْعَ وَ الزَّیْتُوْنَ وَ النَّخِیْلَ وَ الْاَعْنَابَ وَ مِنْ كُلِّ الثَّمَرٰتِؕ-اِنَّ فِیْ ذٰلِكَ لَاٰیَةً لِّقَوْمٍ یَّتَفَكَّرُوْنَ(۱۱)', 0, 2, 8, 14, 3, 1, 16, 'ينبت لكم به الزرع والزيتون والنخيل والأعناب ومن كل الثمرات ۗ إن في ذلك لآية لقوم يتفكرون', 0),
(1928, 12, 'وَ سَخَّرَ لَكُمُ الَّیْلَ وَ النَّهَارَۙ-وَ الشَّمْسَ وَ الْقَمَرَؕ-وَ النُّجُوْمُ مُسَخَّرٰتٌۢ بِاَمْرِهٖؕ-اِنَّ فِیْ ذٰلِكَ لَاٰیٰتٍ لِّقَوْمٍ یَّعْقِلُوْنَۙ(۱۲)', 0, 2, 8, 14, 3, 1, 16, 'وسخر لكم الليل والنهار والشمس والقمر ۖ والنجوم مسخرات بأمره ۗ إن في ذلك لآيات لقوم يعقلون', 0),
(1929, 13, 'وَ مَا ذَرَاَ لَكُمْ فِی الْاَرْضِ مُخْتَلِفًا اَلْوَانُهٗؕ-اِنَّ فِیْ ذٰلِكَ لَاٰیَةً لِّقَوْمٍ یَّذَّكَّرُوْنَ(۱۳)', 0, 2, 8, 14, 3, 1, 16, 'وما ذرأ لكم في الأرض مختلفا ألوانه ۗ إن في ذلك لآية لقوم يذكرون', 0),
(1930, 14, 'وَ هُوَ الَّذِیْ سَخَّرَ الْبَحْرَ لِتَاْكُلُوْا مِنْهُ لَحْمًا طَرِیًّا وَّ تَسْتَخْرِجُوْا مِنْهُ حِلْیَةً تَلْبَسُوْنَهَاۚ-وَ تَرَى الْفُلْكَ مَوَاخِرَ فِیْهِ وَ لِتَبْتَغُوْا مِنْ فَضْلِهٖ وَ لَعَلَّكُمْ تَشْكُرُوْنَ(۱۴)', 0, 2, 8, 14, 3, 1, 16, 'وهو الذي سخر البحر لتأكلوا منه لحما طريا وتستخرجوا منه حلية تلبسونها وترى الفلك مواخر فيه ولتبتغوا من فضله ولعلكم تشكرون', 0),
(1931, 15, 'وَ اَلْقٰى فِی الْاَرْضِ رَوَاسِیَ اَنْ تَمِیْدَ بِكُمْ وَ اَنْهٰرًا وَّ سُبُلًا لَّعَلَّكُمْ تَهْتَدُوْنَۙ(۱۵)', 0, 2, 8, 14, 3, 1, 16, 'وألقى في الأرض رواسي أن تميد بكم وأنهارا وسبلا لعلكم تهتدون', 0),
(1932, 16, 'وَ عَلٰمٰتٍؕ-وَ بِالنَّجْمِ هُمْ یَهْتَدُوْنَ(۱۶)', 0, 2, 8, 14, 3, 1, 16, 'وعلامات ۚ وبالنجم هم يهتدون', 0),
(1933, 17, 'اَفَمَنْ یَّخْلُقُ كَمَنْ لَّا یَخْلُقُؕ-اَفَلَا تَذَكَّرُوْنَ(۱۷)', 0, 2, 8, 14, 3, 1, 16, 'أفمن يخلق كمن لا يخلق ۗ أفلا تذكرون', 0),
(1934, 18, 'وَ اِنْ تَعُدُّوْا نِعْمَةَ اللّٰهِ لَا تُحْصُوْهَاؕ-اِنَّ اللّٰهَ لَغَفُوْرٌ رَّحِیْمٌ(۱۸)', 0, 2, 8, 14, 3, 1, 16, 'وإن تعدوا نعمة الله لا تحصوها ۗ إن الله لغفور رحيم', 0),
(1935, 19, 'وَ اللّٰهُ یَعْلَمُ مَا تُسِرُّوْنَ وَ مَا تُعْلِنُوْنَ(۱۹)', 0, 2, 8, 14, 3, 1, 16, 'والله يعلم ما تسرون وما تعلنون', 0),
(1936, 20, 'وَ الَّذِیْنَ یَدْعُوْنَ مِنْ دُوْنِ اللّٰهِ لَا یَخْلُقُوْنَ شَیْــٴًـا وَّ هُمْ یُخْلَقُوْنَؕ(۲۰)', 0, 2, 8, 14, 3, 1, 16, 'والذين يدعون من دون الله لا يخلقون شيئا وهم يخلقون', 0),
(1937, 21, 'اَمْوَاتٌ غَیْرُ اَحْیَآءٍۚ-وَ مَا یَشْعُرُوْنَۙ-اَیَّانَ یُبْعَثُوْنَ۠(۲۱)', 0, 2, 8, 14, 3, 1, 16, 'أموات غير أحياء ۖ وما يشعرون أيان يبعثون', 0),
(1938, 22, 'اِلٰهُكُمْ اِلٰهٌ وَّاحِدٌۚ-فَالَّذِیْنَ لَا یُؤْمِنُوْنَ بِالْاٰخِرَةِ قُلُوْبُهُمْ مُّنْكِرَةٌ وَّ هُمْ مُّسْتَكْبِرُوْنَ(۲۲)', 0, 3, 9, 14, 3, 1, 16, 'إلهكم إله واحد ۚ فالذين لا يؤمنون بالآخرة قلوبهم منكرة وهم مستكبرون', 0),
(1939, 23, 'لَا جَرَمَ اَنَّ اللّٰهَ یَعْلَمُ مَا یُسِرُّوْنَ وَ مَا یُعْلِنُوْنَؕ-اِنَّهٗ لَا یُحِبُّ الْمُسْتَكْبِرِیْنَ(۲۳)', 0, 3, 9, 14, 3, 1, 16, 'لا جرم أن الله يعلم ما يسرون وما يعلنون ۚ إنه لا يحب المستكبرين', 0),
(1940, 24, 'وَ اِذَا قِیْلَ لَهُمْ مَّا ذَاۤ اَنْزَلَ رَبُّكُمْۙ-قَالُوْۤا اَسَاطِیْرُ الْاَوَّلِیْنَۙ(۲۴)', 0, 3, 9, 14, 3, 1, 16, 'وإذا قيل لهم ماذا أنزل ربكم ۙ قالوا أساطير الأولين', 0),
(1941, 25, 'لِیَحْمِلُوْۤا اَوْزَارَهُمْ كَامِلَةً یَّوْمَ الْقِیٰمَةِۙ-وَ مِنْ اَوْزَارِ الَّذِیْنَ یُضِلُّوْنَهُمْ بِغَیْرِ عِلْمٍؕ-اَلَا سَآءَ مَا یَزِرُوْنَ۠(۲۵)', 0, 3, 9, 14, 3, 1, 16, 'ليحملوا أوزارهم كاملة يوم القيامة ۙ ومن أوزار الذين يضلونهم بغير علم ۗ ألا ساء ما يزرون', 0),
(1942, 26, 'قَدْ مَكَرَ الَّذِیْنَ مِنْ قَبْلِهِمْ فَاَتَى اللّٰهُ بُنْیَانَهُمْ مِّنَ الْقَوَاعِدِ فَخَرَّ عَلَیْهِمُ السَّقْفُ مِنْ فَوْقِهِمْ وَ اَتٰىهُمُ الْعَذَابُ مِنْ حَیْثُ لَا یَشْعُرُوْنَ(۲۶)', 0, 4, 10, 14, 3, 1, 16, 'قد مكر الذين من قبلهم فأتى الله بنيانهم من القواعد فخر عليهم السقف من فوقهم وأتاهم العذاب من حيث لا يشعرون', 0),
