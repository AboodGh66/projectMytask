(4176, 4, 'مَا یُجَادِلُ فِیْۤ اٰیٰتِ اللّٰهِ اِلَّا الَّذِیْنَ كَفَرُوْا فَلَا یَغْرُرْكَ تَقَلُّبُهُمْ فِی الْبِلَادِ(۴)', 0, 1, 6, 24, 6, 1, 40, 'ما يجادل في آيات الله إلا الذين كفروا فلا يغررك تقلبهم في البلاد', 0),
(4177, 5, 'كَذَّبَتْ قَبْلَهُمْ قَوْمُ نُوْحٍ وَّ الْاَحْزَابُ مِنْۢ بَعْدِهِمْ۪-وَ هَمَّتْ كُلُّ اُمَّةٍۭ بِرَسُوْلِهِمْ لِیَاْخُذُوْهُ وَ جٰدَلُوْا بِالْبَاطِلِ لِیُدْحِضُوْا بِهِ الْحَقَّ فَاَخَذْتُهُمْ- فَكَیْفَ كَانَ عِقَابِ(۵)', 0, 1, 6, 24, 6, 1, 40, 'كذبت قبلهم قوم نوح والأحزاب من بعدهم ۖ وهمت كل أمة برسولهم ليأخذوه ۖ وجادلوا بالباطل ليدحضوا به الحق فأخذتهم ۖ فكيف كان عقاب', 0),
(4178, 6, 'وَ كَذٰلِكَ حَقَّتْ كَلِمَتُ رَبِّكَ عَلَى الَّذِیْنَ كَفَرُوْۤا اَنَّهُمْ اَصْحٰبُ النَّارِۘؔ(۶)', 0, 1, 6, 24, 6, 1, 40, 'وكذلك حقت كلمت ربك على الذين كفروا أنهم أصحاب النار', 0),
(4179, 7, 'اَلَّذِیْنَ یَحْمِلُوْنَ الْعَرْشَ وَ مَنْ حَوْلَهٗ یُسَبِّحُوْنَ بِحَمْدِ رَبِّهِمْ وَ یُؤْمِنُوْنَ بِهٖ وَ یَسْتَغْفِرُوْنَ لِلَّذِیْنَ اٰمَنُوْاۚ-رَبَّنَا وَسِعْتَ كُلَّ شَیْءٍ رَّحْمَةً وَّ عِلْمًا فَاغْفِرْ لِلَّذِیْنَ تَابُوْا وَ اتَّبَعُوْا سَبِیْلَكَ وَ قِهِمْ عَذَابَ الْجَحِیْمِ(۷)', 0, 1, 6, 24, 6, 1, 40, 'الذين يحملون العرش ومن حوله يسبحون بحمد ربهم ويؤمنون به ويستغفرون للذين آمنوا ربنا وسعت كل شيء رحمة وعلما فاغفر للذين تابوا واتبعوا سبيلك وقهم عذاب الجحيم', 0),
(4180, 8, 'رَبَّنَا وَ اَدْخِلْهُمْ جَنّٰتِ عَدْنِ ﹰالَّتِیْ وَعَدْتَّهُمْ وَ مَنْ صَلَحَ مِنْ اٰبَآىٕهِمْ وَ اَزْوَاجِهِمْ وَ ذُرِّیّٰتِهِمْؕ-اِنَّكَ اَنْتَ الْعَزِیْزُ الْحَكِیْمُۙ(۸)', 0, 1, 6, 24, 6, 1, 40, 'ربنا وأدخلهم جنات عدن التي وعدتهم ومن صلح من آبائهم وأزواجهم وذرياتهم ۚ إنك أنت العزيز الحكيم', 0),
(4181, 9, 'وَ قِهِمُ السَّیِّاٰتِؕ-وَ مَنْ تَقِ السَّیِّاٰتِ یَوْمَىٕذٍ فَقَدْ رَحِمْتَهٗؕ-وَ ذٰلِكَ هُوَ الْفَوْزُ الْعَظِیْمُ۠(۹)', 0, 1, 6, 24, 6, 1, 40, 'وقهم السيئات ۚ ومن تق السيئات يومئذ فقد رحمته ۚ وذلك هو الفوز العظيم', 0),
(4182, 10, 'اِنَّ الَّذِیْنَ كَفَرُوْا یُنَادَوْنَ لَمَقْتُ اللّٰهِ اَكْبَرُ مِنْ مَّقْتِكُمْ اَنْفُسَكُمْ اِذْ تُدْعَوْنَ اِلَى الْاِیْمَانِ فَتَكْفُرُوْنَ(۱۰)', 0, 2, 7, 24, 6, 1, 40, 'إن الذين كفروا ينادون لمقت الله أكبر من مقتكم أنفسكم إذ تدعون إلى الإيمان فتكفرون', 0),
(4183, 11, 'قَالُوْا رَبَّنَاۤ اَمَتَّنَا اثْنَتَیْنِ وَ اَحْیَیْتَنَا اثْنَتَیْنِ فَاعْتَرَفْنَا بِذُنُوْبِنَا فَهَلْ اِلٰى خُرُوْجٍ مِّنْ سَبِیْلٍ(۱۱)', 0, 2, 7, 24, 6, 1, 40, 'قالوا ربنا أمتنا اثنتين وأحييتنا اثنتين فاعترفنا بذنوبنا فهل إلى خروج من سبيل', 0),
(4184, 12, 'ذٰلِكُمْ بِاَنَّهٗۤ اِذَا دُعِیَ اللّٰهُ وَحْدَهٗ كَفَرْتُمْۚ-وَ اِنْ یُّشْرَكْ بِهٖ تُؤْمِنُوْاؕ-فَالْحُكْمُ لِلّٰهِ الْعَلِیِّ الْكَبِیْرِ(۱۲)', 0, 2, 7, 24, 6, 1, 40, 'ذلكم بأنه إذا دعي الله وحده كفرتم ۖ وإن يشرك به تؤمنوا ۚ فالحكم لله العلي الكبير', 0),
(4185, 13, 'هُوَ الَّذِیْ یُرِیْكُمْ اٰیٰتِهٖ وَ یُنَزِّلُ لَكُمْ مِّنَ السَّمَآءِ رِزْقًاؕ-وَ مَا یَتَذَكَّرُ اِلَّا مَنْ یُّنِیْبُ(۱۳)', 0, 2, 7, 24, 6, 1, 40, 'هو الذي يريكم آياته وينزل لكم من السماء رزقا ۚ وما يتذكر إلا من ينيب', 0),
(4186, 14, 'فَادْعُوا اللّٰهَ مُخْلِصِیْنَ لَهُ الدِّیْنَ وَ لَوْ كَرِهَ الْكٰفِرُوْنَ(۱۴)', 0, 2, 7, 24, 6, 1, 40, 'فادعوا الله مخلصين له الدين ولو كره الكافرون', 0),
(4187, 15, 'رَفِیْعُ الدَّرَجٰتِ ذُو الْعَرْشِۚ-یُلْقِی الرُّوْحَ مِنْ اَمْرِهٖ عَلٰى مَنْ یَّشَآءُ مِنْ عِبَادِهٖ لِیُنْذِرَ یَوْمَ التَّلَاقِۙ(۱۵)', 0, 2, 7, 24, 6, 1, 40, 'رفيع الدرجات ذو العرش يلقي الروح من أمره على من يشاء من عباده لينذر يوم التلاق', 0),
(4188, 16, 'یَوْمَ هُمْ بٰرِزُوْنَ ﳛ لَا یَخْفٰى عَلَى اللّٰهِ مِنْهُمْ شَیْءٌؕ-لِمَنِ الْمُلْكُ الْیَوْمَؕ-لِلّٰهِ الْوَاحِدِ الْقَهَّارِ(۱۶)', 0, 2, 7, 24, 6, 1, 40, 'يوم هم بارزون ۖ لا يخفى على الله منهم شيء ۚ لمن الملك اليوم ۖ لله الواحد القهار', 0),
(4189, 17, 'اَلْیَوْمَ تُجْزٰى كُلُّ نَفْسٍۭ بِمَا كَسَبَتْؕ-لَا ظُلْمَ الْیَوْمَؕ-اِنَّ اللّٰهَ سَرِیْعُ الْحِسَابِ(۱۷)', 0, 2, 7, 24, 6, 1, 40, 'اليوم تجزى كل نفس بما كسبت ۚ لا ظلم اليوم ۚ إن الله سريع الحساب', 0),
(4190, 18, 'وَ اَنْذِرْهُمْ یَوْمَ الْاٰزِفَةِ اِذِ الْقُلُوْبُ لَدَى الْحَنَاجِرِ كٰظِمِیْنَ۬ؕ-مَا لِلظّٰلِمِیْنَ مِنْ حَمِیْمٍ وَّ لَا شَفِیْعٍ یُّطَاعُؕ(۱۸)', 0, 2, 7, 24, 6, 1, 40, 'وأنذرهم يوم الآزفة إذ القلوب لدى الحناجر كاظمين ۚ ما للظالمين من حميم ولا شفيع يطاع', 0),
(4191, 19, 'یَعْلَمُ خَآىٕنَةَ الْاَعْیُنِ وَ مَا تُخْفِی الصُّدُوْرُ(۱۹)', 0, 2, 7, 24, 6, 1, 40, 'يعلم خائنة الأعين وما تخفي الصدور', 0),
(4192, 20, 'وَ اللّٰهُ یَقْضِیْ بِالْحَقِّؕ-وَ الَّذِیْنَ یَدْعُوْنَ مِنْ دُوْنِهٖ لَا یَقْضُوْنَ بِشَیْءٍؕ-اِنَّ اللّٰهَ هُوَ السَّمِیْعُ الْبَصِیْرُ۠(۲۰)', 0, 2, 7, 24, 6, 1, 40, 'والله يقضي بالحق ۖ والذين يدعون من دونه لا يقضون بشيء ۗ إن الله هو السميع البصير', 0),
(4193, 21, 'اَوَ لَمْ یَسِیْرُوْا فِی الْاَرْضِ فَیَنْظُرُوْا كَیْفَ كَانَ عَاقِبَةُ الَّذِیْنَ كَانُوْا مِنْ قَبْلِهِمْؕ-كَانُوْا هُمْ اَشَدَّ مِنْهُمْ قُوَّةً وَّ اٰثَارًا فِی الْاَرْضِ فَاَخَذَهُمُ اللّٰهُ بِذُنُوْبِهِمْؕ -وَ مَا كَانَ لَهُمْ مِّنَ اللّٰهِ مِنْ وَّاقٍ(۲۱)', 0, 3, 8, 24, 6, 1, 40, 'أولم يسيروا في الأرض فينظروا كيف كان عاقبة الذين كانوا من قبلهم ۚ كانوا هم أشد منهم قوة وآثارا في الأرض فأخذهم الله بذنوبهم وما كان لهم من الله من واق', 0),
(4194, 22, 'ذٰلِكَ بِاَنَّهُمْ كَانَتْ تَّاْتِیْهِمْ رُسُلُهُمْ بِالْبَیِّنٰتِ فَكَفَرُوْا فَاَخَذَهُمُ اللّٰهُؕ-اِنَّهٗ قَوِیٌّ شَدِیْدُ الْعِقَابِ(۲۲)', 0, 3, 8, 24, 6, 1, 40, 'ذلك بأنهم كانت تأتيهم رسلهم بالبينات فكفروا فأخذهم الله ۚ إنه قوي شديد العقاب', 0),
(4195, 23, 'وَ لَقَدْ اَرْسَلْنَا مُوْسٰى بِاٰیٰتِنَا وَ سُلْطٰنٍ مُّبِیْنٍۙ(۲۳)', 0, 3, 8, 24, 6, 1, 40, 'ولقد أرسلنا موسى بآياتنا وسلطان مبين', 0),
(4196, 24, 'اِلٰى فِرْعَوْنَ وَ هَامٰنَ وَ قَارُوْنَ فَقَالُوْا سٰحِرٌ كَذَّابٌ(۲۴)', 0, 3, 8, 24, 6, 1, 40, 'إلى فرعون وهامان وقارون فقالوا ساحر كذاب', 0),
(4197, 25, 'فَلَمَّا جَآءَهُمْ بِالْحَقِّ مِنْ عِنْدِنَا قَالُوا اقْتُلُوْۤا اَبْنَآءَ الَّذِیْنَ اٰمَنُوْا مَعَهٗ وَ اسْتَحْیُوْا نِسَآءَهُمْؕ-وَ مَا كَیْدُ الْكٰفِرِیْنَ اِلَّا فِیْ ضَلٰلٍ(۲۵)', 0, 3, 8, 24, 6, 1, 40, 'فلما جاءهم بالحق من عندنا قالوا اقتلوا أبناء الذين آمنوا معه واستحيوا نساءهم ۚ وما كيد الكافرين إلا في ضلال', 0),
(4198, 26, 'وَ قَالَ فِرْعَوْنُ ذَرُوْنِیْۤ اَقْتُلْ مُوْسٰى وَ لْیَدْعُ رَبَّهٗۚ-اِنِّیْۤ اَخَافُ اَنْ یُّبَدِّلَ دِیْنَكُمْ اَوْ اَنْ یُّظْهِرَ فِی الْاَرْضِ الْفَسَادَ(۲۶)', 0, 3, 8, 24, 6, 1, 40, 'وقال فرعون ذروني أقتل موسى وليدع ربه ۖ إني أخاف أن يبدل دينكم أو أن يظهر في الأرض الفساد', 0),
(4199, 27, 'وَ قَالَ مُوْسٰۤى اِنِّیْ عُذْتُ بِرَبِّیْ وَ رَبِّكُمْ مِّنْ كُلِّ مُتَكَبِّرٍ لَّا یُؤْمِنُ بِیَوْمِ الْحِسَابِ۠(۲۷)', 0, 3, 8, 24, 6, 1, 40, 'وقال موسى إني عذت بربي وربكم من كل متكبر لا يؤمن بيوم الحساب', 0);
INSERT INTO `quran` (`ayatId`, `ayatNumber`, `arabicText`, `ayatSajda`, `surahRuku`, `paraRuku`, `paraId`, `manzilNo`, `ayatVisible`, `surahId`, `withoutAerab`, `favourite`) VALUES
(4200, 28, 'وَ قَالَ رَجُلٌ مُّؤْمِنٌ ﳓ مِّنْ اٰلِ فِرْعَوْنَ یَكْتُمُ اِیْمَانَهٗۤ اَتَقْتُلُوْنَ رَجُلًا اَنْ یَّقُوْلَ رَبِّیَ اللّٰهُ وَ قَدْ جَآءَكُمْ بِالْبَیِّنٰتِ مِنْ رَّبِّكُمْؕ-وَ اِنْ یَّكُ كَاذِبًا فَعَلَیْهِ كَذِبُهٗۚ-وَ اِنْ یَّكُ صَادِقًا یُّصِبْكُمْ بَعْضُ الَّذِیْ یَعِدُكُمْؕ-اِنَّ اللّٰهَ لَا یَهْدِیْ مَنْ هُوَ مُسْرِفٌ كَذَّابٌ(۲۸)', 0, 4, 9, 24, 6, 1, 40, 'وقال رجل مؤمن من آل فرعون يكتم إيمانه أتقتلون رجلا أن يقول ربي الله وقد جاءكم بالبينات من ربكم ۖ وإن يك كاذبا فعليه كذبه ۖ وإن يك صادقا يصبكم بعض الذي يعدكم ۖ إن الله لا يهدي من هو مسرف كذاب', 0),
(4201, 29, 'یٰقَوْمِ لَكُمُ الْمُلْكُ الْیَوْمَ ظٰهِرِیْنَ فِی الْاَرْضِ٘-فَمَنْ یَّنْصُرُنَا مِنْۢ بَاْسِ اللّٰهِ اِنْ جَآءَنَاؕ-قَالَ فِرْعَوْنُ مَاۤ اُرِیْكُمْ اِلَّا مَاۤ اَرٰى وَ مَاۤ اَهْدِیْكُمْ اِلَّا سَبِیْلَ الرَّشَادِ(۲۹)', 0, 4, 9, 24, 6, 1, 40, 'يا قوم لكم الملك اليوم ظاهرين في الأرض فمن ينصرنا من بأس الله إن جاءنا ۚ قال فرعون ما أريكم إلا ما أرى وما أهديكم إلا سبيل الرشاد', 0),
(4202, 30, 'وَ قَالَ الَّذِیْۤ اٰمَنَ یٰقَوْمِ اِنِّیْۤ اَخَافُ عَلَیْكُمْ مِّثْلَ یَوْمِ الْاَحْزَابِۙ(۳۰)', 0, 4, 9, 24, 6, 1, 40, 'وقال الذي آمن يا قوم إني أخاف عليكم مثل يوم الأحزاب', 0),
(4203, 31, 'مِثْلَ دَاْبِ قَوْمِ نُوْحٍ وَّ عَادٍ وَّ ثَمُوْدَ وَ الَّذِیْنَ مِنْۢ بَعْدِهِمْؕ-وَ مَا اللّٰهُ یُرِیْدُ ظُلْمًا لِّلْعِبَادِ(۳۱)', 0, 4, 9, 24, 6, 1, 40, 'مثل دأب قوم نوح وعاد وثمود والذين من بعدهم ۚ وما الله يريد ظلما للعباد', 0),
(4204, 32, 'وَ یٰقَوْمِ اِنِّیْۤ اَخَافُ عَلَیْكُمْ یَوْمَ التَّنَادِۙ(۳۲)', 0, 4, 9, 24, 6, 1, 40, 'ويا قوم إني أخاف عليكم يوم التناد', 0),
(4205, 33, 'یَوْمَ تُوَلُّوْنَ مُدْبِرِیْنَۚ-مَا لَكُمْ مِّنَ اللّٰهِ مِنْ عَاصِمٍۚ-وَ مَنْ یُّضْلِلِ اللّٰهُ فَمَا لَهٗ مِنْ هَادٍ(۳۳)', 0, 4, 9, 24, 6, 1, 40, 'يوم تولون مدبرين ما لكم من الله من عاصم ۗ ومن يضلل الله فما له من هاد', 0),
(4206, 34, 'وَ لَقَدْ جَآءَكُمْ یُوْسُفُ مِنْ قَبْلُ بِالْبَیِّنٰتِ فَمَا زِلْتُمْ فِیْ شَكٍّ مِّمَّا جَآءَكُمْ بِهٖؕ-حَتّٰۤى اِذَا هَلَكَ قُلْتُمْ لَنْ یَّبْعَثَ اللّٰهُ مِنْۢ بَعْدِهٖ رَسُوْلًاؕ-كَذٰلِكَ یُضِلُّ اللّٰهُ مَنْ هُوَ مُسْرِفٌ مُّرْتَابُۚۖﰳ(۳۴)', 0, 4, 9, 24, 6, 1, 40, 'ولقد جاءكم يوسف من قبل بالبينات فما زلتم في شك مما جاءكم به ۖ حتى إذا هلك قلتم لن يبعث الله من بعده رسولا ۚ كذلك يضل الله من هو مسرف مرتاب', 0),
(4207, 35, 'الَّذِیْنَ یُجَادِلُوْنَ فِیْۤ اٰیٰتِ اللّٰهِ بِغَیْرِ سُلْطٰنٍ اَتٰىهُمْؕ-كَبُرَ مَقْتًا عِنْدَ اللّٰهِ وَ عِنْدَ الَّذِیْنَ اٰمَنُوْاؕ-كَذٰلِكَ یَطْبَعُ اللّٰهُ عَلٰى كُلِّ قَلْبِ مُتَكَبِّرٍ جَبَّارٍ(۳۵)', 0, 4, 9, 24, 6, 1, 40, 'الذين يجادلون في آيات الله بغير سلطان أتاهم ۖ كبر مقتا عند الله وعند الذين آمنوا ۚ كذلك يطبع الله على كل قلب متكبر جبار', 0),
(4208, 36, 'وَ قَالَ فِرْعَوْنُ یٰهَامٰنُ ابْنِ لِیْ صَرْحًا لَّعَلِّیْۤ اَبْلُغُ الْاَسْبَابَۙ(۳۶)', 0, 4, 9, 24, 6, 1, 40, 'وقال فرعون يا هامان ابن لي صرحا لعلي أبلغ الأسباب', 0),
